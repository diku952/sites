    var formCallBackFunction = {

        widget : null,

        submitForm: function () {

            var form = $('.callback_block_form');

            if(form.valid()) {
                grecaptcha.execute(formCallBackFunction.widget);
                form.trigger('submit');
            } else {
                grecaptcha.reset(formCallBackFunction.widget);
            }

        },

    };

    function onLoadRecaptcha() {
        formCallBackFunction.widget = grecaptcha.render('submit_callback_bloc', {
            'sitekey': "6LcYr1gUAAAAAFZRsgxFbC4RIwAheb_rMPFquYBC",
            'callback': formCallBackFunction.submitForm,
        });

        if ($('#gRecaptchaResponse').length) {
            onLoadRecaptchaAuth(); // Auth
        }
    }

