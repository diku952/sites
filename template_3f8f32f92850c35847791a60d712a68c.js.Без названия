        width: "100%",
        selectedList: 1,
        groupSelected: !1,
        checkAllText: "Выбрать все",
        noneSelectedText: function noneSelectedText() {
            return $(this).attr("placeholder");
        }
    }), $(".f-select").filterSelector({
        multiple: !1,
        header: !1,
        displaySelectedText: !0,
        filter: !0,
        width: "100%",
        selectedList: 1,
        groupSelected: !1,
        checkAllText: "Выбрать все",
        noneSelectedText: "Выберите из списка"
    });
    var e = $(".js-scroll");
    e.mCustomScrollbar({ theme: "rec" });
    var t = $(".js-checkbox-custom"),
        l = $(".js-input-custom");
    t.on("click", function () {
        l.toggleClass("hidden");
    });
});

// Company logo

$(document).ready(function () {

    function clearForm() {
        var selects = $('#partnerListFilterCountry, #partnerListFilterFor');
        $('*[data-partners-list-form]').trigger('reset');
        $('.js-clear-filters').trigger('click');
        showClearFilters = false;
        selects.attr('disabled', 'disabled');
        try {
            selects.select2('destroy').select2({
                theme: 'rec',
                minimumResultsForSearch: Infinity,
                width: '100%',
                language: {
                    noResults: function noResults() {
                        return "Не найдено";
                    }
                }
            });
        } catch (e) {
            //console.log(e)
        }
        $('*[data-step-select]').val([]).trigger('change');
    }

    clearForm();

    function readURL(input, img) {

        if (input.files && input.files[0]) {
            $('.company-form').append('<input type="file" name="PREVIEW_PICTURE" class="js-company-form-logo hidden-input" />');
            $('.js-company-form-logo')[0].files = input.files;

            var reader = new FileReader();

            reader.onload = function (e) {
                $(img).attr('src', e.target.result).removeClass('company-logo__error');
            };

            reader.readAsDataURL(input.files[0]);
        }
    }
    $('.js-company-logo__input').change(function () {
        readURL(this, $(this).parent().find('.company-logo__block-img'));
    });

    // country select
    $('.js-select2.js-country-select:not(.mobile-select)').select2({
        theme: 'rec',
        minimumResultsForSearch: Infinity,
        width: '100%',
        language: {
            noResults: function noResults() {
                return "Не найдено";
            }
        }
    }).on('select2:open', function () {
        $('.select2-container--rec .select2-dropdown--below').mCustomScrollbar({
            theme: 'rec',
            mouseWheel: true
        });
        $(".select2-container--rec .select2-results ul.select2-results__options").unbind("mousewheel");
    });

    // Form validation
    $('*[data-validatable]').each(function () {
        var context = this;
        // if false = invalid
        var setElementValidationState = function setElementValidationState(element, state) {
            if (!state) $(element).addClass('amx-form__element--ivalid');else $(element).removeClass('amx-form__element--ivalid');
        };

        var validate = function validate(th) {
            $(th).find('*[data-validate-rule]').each(function () {
                var attr = $(this).attr('data-validate-disabled');
                if ((typeof attr === "undefined" ? "undefined" : _typeof(attr)) !== (typeof undefined === "undefined" ? "undefined" : _typeof(undefined)) && attr !== false) {
                    setElementValidationState($(this), true);
                    return;
                }
                var isFormElementValid = true;
                var schema = JSON.parse($(this).attr('data-validate-rule'));

                // Валидация min (длина)
                if (schema.min && $(this).val().length < schema.min) {
                    setElementValidationState($(this), false);
                    return;
                }

                // Валидация рейтинга по инпуту
                if (schema.ratingRequired && !$(this).attr('data-score')) {
                    setElementValidationState($(this), false);
                    return;
                }
                setElementValidationState($(this), true);
            });
            return !$(th).find('.amx-form__element--ivalid').length;
        };

        var $submitButton = $(this).find('*[data-validate-submit]');
        function evalValidation() {
            if (validate(context)) {
                $submitButton.removeAttr('disabled');
            } else {
                $submitButton.attr('disabled', '');
            }
        }
        $(context).on('refresh-bindings', function () {
            $(context).find('input,textarea,select').off('keyup change', evalValidation).on('keyup change', evalValidation);
            $(context).find('.js-estimate-list img').off('click', evalValidation).on('click', evalValidation);
        });

        $(context).trigger('refresh-bindings');
    });

    $('*[data-js-open-layer=assess01]').click(function () {
        setTimeout(function () {
            $('*[data-validatable]').trigger('refresh-bindings');
        }, 500);
    });

    function refrechLinks() {
        var links = $('.partner_list_ajax .pagination-circle a');
        $.each(links, function (e) {
            $(this).on('click', function (i) {
                i.preventDefault();
                var link = getParameterByName('PAGEN_1', $(this).attr('href'));
                $('input[name="PAGEN_1"]').val(link);
                var $form = $('*[data-partners-list-form]');
                $.ajax({
                    type: $form.attr('method'),
                    url: $form.attr('action'),
                    data: $form.serialize()
                }).done(function (data) {
                    $('.partner_list_ajax').html(data);
                });
            });
        });
    }
    refrechLinks();
    $('[name="direction"]').on('change', function (e) {
        $('input[name="PAGEN_1"]').val('1');
        if ($('[name="services"]').val() !== '') {
            $('[name="CLEAR_SERVICE"]').attr('value', 'Y');
        }
        $('[name="services"]').attr('value', '');
        refrechLinks();
    });
    $('[name="services"]').on('change', function (e) {
        $('input[name="PAGEN_1"]').val('1');

        $('[name="CLEAR_SERVICE"]').attr('value', '');
        refrechLinks();
    });

    // AJAX form submit
    $('*[data-ajax-form]').submit(function (e) {
        var $form = $(this);
        e.preventDefault();
        setTimeout(function (e) {
            $.ajax({
                type: $form.attr('method'),
                url: $form.attr('action'),
                data: $form.serialize()
            }).done(function (rsp) {
                $form.trigger('ajax-success');
                $('*[data-amx-form-replacable]').html('<div class="amx-form__ty-message">Ваша заявка успешно отозвана.</div>');
            });
        }, 500);
    });

    // Логика работы селектов в Аккредитированных партнерах
    var ignoreRefreshing = false;

    if ($('#partnerListFilter').val() && $('#partnerListFilterFor').val()) {
        refreshPartnersList();
    }

    $('*[data-partners-list-form]').on('form_submit', function (object) {
        refreshPartnersList();
    });

    function refreshPartnersList(hide) {
        if (ignoreRefreshing) return;
        var $form = $('*[data-partners-list-form]');
        var dir = $('[name="direction"]').val();
        var $country = $form.find('#partnerListFilterCountry');
        var $listCountry = $form.find('#partnerListFilterCountry option');
        var $application = $('#partnerListFilterFor');
        var specialization = $('#partnerListFilterFor').val();
        var directionVal = $('#partnerListFilter').val();
        var countryVal = $('#partnerListFilterCountry').val();
        var dataStepOptCount = $('*[data-step-select=1] option:enabled').length;
        
        // Если при выборе в селекте ВЫБЕРЕТЕ НАПРАВЛЕНИЕ что то было выбрано, то убираем обводку красным бордером
        if (directionVal && directionVal.length > 0) {
            $('#partnerListFilter').siblings('.select2-container').removeClass('error');
            // Если при выборе в селекте ВЫБЕРЕТЕ УСЛУГУ ничего не выбрано, то добавляем обводку красным бордером
            // но также учитываем что в селекте ВЫБЕРЕТЕ УСЛУГУ больше одного значения
            // и в урле нет прямого указания id услуги или был сброс фильтра
            if ( (!specialization || specialization.length == 0 || specialization == null) && (!GetURLParameter('service') || window.clearFilter) && dataStepOptCount != 1) { 
                setTimeout(function () {
                    $('#partnerListFilterFor').siblings('.select2-container').addClass('error');
                }, 100);
            }
        }
        
        // Если при выборе в селекте ВЫБЕРЕТЕ УСЛУГУ что то было выбрано, то убираем обводку красным бордером
        if (specialization && specialization.length > 0) {
            $('#partnerListFilterFor').siblings('.select2-container').removeClass('error');
        }    

        // Если при выборе в селекте ВЫБЕРЕТЕ СТРАНУ НАЗНАЧЕНИЯ что то было выбрано, то убираем обводку красным бордером
        if (countryVal && countryVal.length > 0) {
            $('#partnerListFilterCountry').siblings('.select2-container').removeClass('error');
        }        

        window.amxSpecializationList = [];

        // scroll
        $('#js-scroll').attr('js-scroll', 'filter');
        $.ajax({
            url: $form.attr('action'),
            type: $form.attr('method'),
            data: $form.serialize(),
            success: function success(data) {
                $('.partner_list_ajax').html(data);
                ignoreRefreshing = false;
                showClearFilters = true;
                //if (GetURLParameter('directions') && GetURLParameter('services') || $(data).find('.partner-item').length > 0) {
                // Сделал чтобы кнопка отправки группового запроса активировалась только если выбрана страна и есть партнеры которые оказывают услуги   
                if (countryVal && countryVal.length > 0 && $(data).find('.partner-item').length > 0) {
                    setTimeout(function () {
                        $('.request-service-button').removeAttr('disabled');
                    }, 500);
                }
            }
        });

        //отправим второй запрос что бы отфильтровать страны
        $.ajax({
            url: $form.attr('action'),
            type: $form.attr('method'),
            data: { direction: dir, partners_type: 'countries', services: specialization },

            success: function success(data) {
                window.amxSpecializationList.push(data.countries);
                var allDis = true;
                //пройдемся по списку стран что бы убрать ненужные
                $listCountry.each(function (i) {
                    //для начала дадим всем disabled (что бы скрыть их)
                    if ($(this).val() !== '' || $(this).val() === null) $(this).attr('disabled', 'disabled');
                    for (var key in data.countries) {
                        //если элемент соответствует пришедшему в ответе сервера уберём disabled
                        if ($(this).val() === data.countries[key]) {
                            $(this).removeAttr('disabled');
                            allDis = false;
                            if ($('#js-amx-not-found')) $('#js-amx-not-found').remove();
                        }
                    }
                    //если все с disabled добавим один
                    if ($listCountry.length === i + 1) {
                        if (allDis) {
                            $country.append('<option id="js-amx-not-found">Не найдено</option>');
                        }

                        /*if ($country.data('select2')) {
                            $country.select2('destroy');
                        }*/
                        $('#partnerListFilterCountry:not(.mobile-select)').select2({
                            theme: 'rec',
                            minimumResultsForSearch: Infinity,
                            width: '100%',
                            language: { noResults: function noResults() {
                                    return "Не найдено";
                                } }
                        });
                    }
                });
                $application.find('option').each(function (i) {
                    if ($(this).data('dir') == dir) {
                        $(this).removeAttrs('disabled');
                    } else {
                        $(this).attr('disabled', 'disabled');
                    }
                });

                window.amxSpecializationFilter = [];
                window.amxSpecializationFilterFlag = 1;
                //запишим в глобальную переменную услуги которые нужно заблокировать в модальном окне
                $listCountry.each(function () {
                    if ($(this).val() !== data.services) {
                        window.amxSpecializationFilter.push($(this).val());
                    }
                }); 
            },
            complete: function() {
                // Если при выборе в селекте ВЫБЕРЕТЕ УСЛУГУ что то было выбрано, то убираем обводку красным бордером
                setTimeout(function () {
                    var specializationVal = $('#partnerListFilterFor').val();
                    if (specializationVal && specializationVal.length > 0) {
                        // Если при выборе в селекте ВЫБЕРЕТЕ СТРАНУ НАЗНАЧЕНИЯ ничего нет то устанавливаем обводку красным бордером
                        if (!countryVal || countryVal.length == 0) { 
                            setTimeout(function () {
                                $('#partnerListFilterCountry').siblings('.select2-container').addClass('error');
                            }, 100);
                        }
                    }
                }, 200);
            }
        });
        refrechLinks();
    };

    function GetURLParameter(sParam) {
        var sPageURL = window.location.search.substring(1);
        var sURLVariables = sPageURL.split('&');
        for (var i = 0; i < sURLVariables.length; i++) {
            var sParameterName = sURLVariables[i].split('=');
            if (sParameterName[0] == sParam) {
                var lastChar = sParameterName[1].substr(sParameterName[1].length - 1); 
                // Поправил чейто старый баг когда в конце урла приходил cлеш
                if (lastChar == '/') {
                    return sParameterName[1].substr(0, sParameterName[1].length - 1);
                } else {
                    return sParameterName[1];
                }
                
            }
        }
        return false;
    };

    $('*[data-step-selects]').each(function () {
        var th = this;
        var disableNext = false;
        var $stepSelects = $(th).find('*[data-step-select]');
        var $country = $('#partnerListFilterCountry');
        // var $application = $('#partnerListFilterFor');
        // var $countryModal = $('.select2-container');

        $stepSelects.each(function (index, item) {

            if (disableNext) {
                /*$reqBtn.attr('disabled','disabled');*/
                $(this).attr('disabled', 'disabled');
            }
            if (!$(this).val()) disableNext = true;
        }).change(function () {
            var changeDirection = false;
            //управляем отображением кнопки "запрос на оказание услуг
            var index = parseInt($(this).attr('data-step-select'));
            if (index === 0 && $(this).val() !== null) {
                changeDirection = true;
                // $reqBtn.css('visibility', 'hidden');
                //если изменилось направление очистим поле "Страны"
                $country.val([]);
            }
            /*if (index === 1 && $(this).val() !== null) {
                $reqBtn.css('visibility', 'visible');
            }*/
            $stepSelects.each(function () {
                var value = $(th).find('*[data-step-select=' + (parseInt($(this).attr('data-step-select')) - 1) + ']').val();
                if (value != null && value.length) $(this).removeAttr('disabled');
                if (changeDirection) {
                    $('#partnerListFilterCountry:not(.mobile-select)').attr('disabled', 'disabled').select2({ //.select2('destroy')
                        theme: 'rec',
                        minimumResultsForSearch: Infinity,
                        width: '100%',
                        language: { noResults: function noResults() {
                                return "Не найдено";
                            } }
                    });                   
                }
            });

            // Добавлена обертка с задержкой так как при кейзе:
            // 1. Выбор направлени 2. Выбор услуга 3. Выбор страны 4. Выбор направления
            // функция refreshPartnersList отрабатывала быстрее чем повторная реинициализация select2
            // и значения в селектах услуг и стран не успевали очиститься
            setTimeout(function () {
                refreshPartnersList();
                ignoreRefreshing = true;
                if (showClearFilters) $('.js-clear-filters').show();
            }, 10);            
            
        });
    });

    // Установка значений фильтра на первый option по умолчанию
    // если нет GET параметров
    if (location.search == ''){
        var partnerListFilter = $('#partnerListFilter'),
            partnerListFilterFor = $('#partnerListFilterFor'),
            partnerListFilterCountry = $('#partnerListFilterCountry');

        var select2PartnerListFilterContainerSpan = $('#select2-partnerListFilter-container span'),
            select2PartnerListFilterForContainerSpan = $('#select2-partnerListFilterFor-container span'),
            select2PartnerListFilterCountryContainerSpan = $('#select2-partnerListFilterCountry-container span');

        partnerListFilter.find('option:eq(1)').attr('selected', 'selected');
        select2PartnerListFilterContainerSpan.text(partnerListFilter.find('option:eq(1)').text());

        partnerListFilterFor.removeAttr('disabled');
        partnerListFilterFor.find('option:eq(1)').attr('selected', 'selected');
        select2PartnerListFilterForContainerSpan.text(partnerListFilterFor.find('option:eq(1)').text());

        partnerListFilterCountry.removeAttr('disabled');
        partnerListFilterCountry.find('option:eq(1)').attr('selected', 'selected');
        select2PartnerListFilterCountryContainerSpan.text(partnerListFilterCountry.find('option:eq(1)').text());

        refreshPartnersList();
    }

    var urlDirections = GetURLParameter('directions');
    var urlServices = GetURLParameter('service');
    var url_register = GetURLParameter('register');

    if (urlDirections) {
        setTimeout(function () {
            var stepSelects = $('*[data-step-select]');
            stepSelects.each(function () {
                var _this = $(this);

                _this.val(urlDirections).trigger('change');
                /*if(_this.attr('name') === 'direction') {
                        _this.select2({
                            theme: 'rec',
                            minimumResultsForSearch: Infinity,
                            width: '100%',
                            language: { noResults: function noResults() {
                                    return "Не найдено";
                            }
                    }
                });
                }*/
            });

            $('.js-clear-filters').show();
        }, 500);

        if (urlServices) {
            setTimeout(function () {
                $('#partnerListFilterFor').val(urlServices).trigger('change');
            }, 1000);
        } else {
            setTimeout(function () {
                var $dataStepOpt = $('*[data-step-select=1] option:enabled');
                var dataStep1 = $dataStepOpt.length;
                if (dataStep1 && dataStep1 == 1) {
                    $('#partnerListFilterFor').val($dataStepOpt.eq(0).val()).trigger('change');
                };
            }, 1100);
        }
    }
    if (url_register) {
        if ($('.request-service-button[data-js-open-layer="auth"]').length && $(".js-layer-register").length) {
            setTimeout(function () {
                $('.request-service-button[data-js-open-layer="auth"]').trigger('click');
                setTimeout(function () {
                    $(".js-layer-register").trigger('click');
                }, 300);
            }, 2300);
        }
    }

    // multiselect
    $('.amx-multiselect select').change(function () {
        var selections = $(this).select2('data');
        if (selections.length > 1) {
            var $item = $('.select2-selection__choice').eq(0);
            var newVal = $item.text($item.clone().children().remove().end().text() + ' +' + --selections.length);
        }
    });

    var $reqBtn = $('.request-service-button[data-js-open-layer]');
    // Обработчик отчистки фильтров
    var showClearFilters = true; //поставим флаг т.к. change занят
    $('.js-clear-filters').click(function () {
        showClearFilters = false;
        window.clearFilter = 1;
        $('#partnerListFilter,#partnerListFilterCountry,#partnerListFilterFor').val('');
        $('.js-select2.mobile-select').nextAll('.mobile-placeholder').show();
        $('#partnerListFilterCountry,#partnerListFilterFor').attr('disabled', 'disabled');
        $('#partnerListFilterCountry,#partnerListFilterFor').select2('destroy').select2({
            theme: 'rec',
            minimumResultsForSearch: Infinity,
            width: '100%',
            language: { noResults: function noResults() {
                    return "Не найдено";
                } }
        });

        // Если при сбросе в селекте выберете направление ничего не выбрано, то подсветим его красным бордером
        if ($('#partnerListFilter').val() == null || $('#partnerListFilter').val() == '') {
            $('#partnerListFilter').siblings('.select2-container').addClass('error');
        }

        $('*[data-step-select]').val([]).trigger('change');
        refreshPartnersList();
        $(this).hide();

        $reqBtn.attr('disabled', 'disabled');
    });
    //плейсхолдер для мобильных в фильтре
    $('.js-select2.mobile-select').on('change', function () {
        if (this.value) $(this).nextAll('.mobile-placeholder').hide();else $(this).nextAll('.mobile-placeholder').show();
    });
    $('#partnerListFilter.mobile-select').on('change', function () {
        $('#partnerListFilterCountry').attr('disabled', 'disabled');
        $('#partnerListFilterCountry,#partnerListFilterFor').val('').trigger('change');
    });
    var $countryModal = $('#country');
    var $listCountryModal = $('#country option');

    //menu toggle
    var $menuItem = $('.js-item-menu');
    $menuItem.on('click', function (e) {
        e.preventDefault();

        var $t = $(this);

        $t.toggleClass('is-open');
        $t.closest('li').toggleClass('is-open');
    });

    $reqBtn.click(function () {
        setTimeout(function () {

            var allDis = true;
            var c = Object.values(window.amxSpecializationList[0]);

            $listCountryModal.each(function (i) {
                if ($(this).val() !== '' || $(this).val() === null) {
                    $(this).attr('disabled', 'disabled');
                }
                if (c.indexOf($(this).val()) !== -1) {
                    $(this).removeAttr('disabled');
                    allDis = false;
                    if ($('#js-amx-not-found')) $('#js-amx-not-found').remove();
                }

                if ($listCountryModal.length === i + 1) {
                    if (allDis) {
                        $countryModal.append('<option id="js-amx-not-found">Не найдено</option>');
                    }
                }
            });

            if ($countryModal.data('select2')) {
                $countryModal.select2('destroy');
            }
            $countryModal.select2({
                theme: 'rec',
                minimumResultsForSearch: Infinity,
                width: '100%',
                language: { noResults: function noResults() {
                        return "Не найдено";
                    } }
            });
        }, 500);
    });

    // Фильтрация options при выборе направления в селекте услуг
    $('*[data-step-select=0]').change(function () {
        var dir = $(this).val();
        $('*[data-step-select=1] option').each(function () {
            $(this).prop('disabled', $(this).attr('data-dir') !== dir);
        });
        setTimeout(function () {
            var instance = $('*[data-step-select=1]').data('select2');
            if (instance) {
                var opts = instance.options.options;
                var $dataStepOpt = $('*[data-step-select=1] option:enabled');
                var dataStep1 = $dataStepOpt.length;
                if (dataStep1 && dataStep1 == 1) {
                    $('*[data-step-select=1]').val($dataStepOpt.eq(0).val()).trigger('change').select2('destroy').select2(opts);
                } else {
                    $('*[data-step-select=1]').val([]).trigger('change').select2('destroy').select2(opts);
                };                
            }
        }, 0);
    });

    // Фильтр - Выбор значения в селекте услуг
    // убираем этот блок так как логика показа кнопки запроса поменялась
    $('*[data-step-select=1]').change(function () {
        var check = $(this).val();
        setTimeout(function () {
            var list = $('.partner-list .partner-item').length;
            if (check == null || list == 0) {
                //$reqBtn.attr('disabled', 'disabled');
            } else {
                // При переходе на адрес /partners/list/?directions=60303&service=67758
                // проиходит заполнение/генерация селекста и выделение бордером происходит не верно
                // поэтому тут делаем повторную проверку и убераем бордер с услуг
                $('#partnerListFilterFor').siblings('.select2-container').removeClass('error');
                //$reqBtn.removeAttrs('disabled');
            }
        }, 500);
    });

    // Замена и удаление файла в заявке
    $('.js-app-attached__action-change').click(function () {
        $(this).parents('form').find('input[type=file]').trigger('click');
    });

    $('.js-app-attached__action-delete').click(function () {
        $(this).parents('form').find('input[type=file]').val('').trigger('change');
    });

    $('.js-app-attached-form input[type=file]').on('change', function () {
        $('.complete-btn').removeAttr('disabled');
    });

    // удаление кнопки Отозвать заявку при саксессе соотв формы
    $('.js-revoke-form').on('ajax-success', function () {
        $('a[data-js-open-layer=cancel]').remove();
        $(window).on('updateToTop', function () {
            location.reload();
        });
    });

    $.fn.bindFirst = function (name, fn) {
        var elem, handlers, i, _len;
        this.bind(name, fn);
        for (i = 0, _len = this.length; i < _len; i++) {
            elem = this[i];
            handlers = jQuery._data(elem).events[name.split('.')[0]];
            handlers.unshift(handlers.pop());
        }
    };

    $('*[data-js-open-layer=to-archive]').click(function () {
        setTimeout(function () {
            $('.js-archive-reason').bindFirst('change', function () {
                if ($(this).find('option:selected')[0].hasAttribute('data-show-textarea')) {
                    $(this).parents('form').find('.app-message__txt').removeAttr('data-validate-disabled', false);
                } else {
                    $(this).parents('form').find('.app-message__txt').attr('data-validate-disabled', '');
                }
            });
        }, 500);
    });

    $('.js-refresh-on-success').on('ajax-success', function () {
        location.reload();
    });
    //написать сообщение
    $('.js-amx-write-msg').click(function () {
        $('html').animate({
            scrollTop: $(".js-tabs__block").offset().top - 60
        }, 2000, function () {
            $('.js-amx-msg-textarea').focus();
        });
    });
    //отправка формы с фйлом
    $('.js-amx-form-evaluate').submit(function (e) {
        e.preventDefault();
        var $form = $(this);
        var formData = new FormData($form[0]);
        $.ajax({
            type: $form.attr('method'),
            url: $form.attr('action'),
            data: formData,
            contentType: false,
            processData: false
        }).done(function (rsp) {
            $form.trigger('ajax-success');
            $('.open_rate_window').hide();
            $('*[data-amx-form-replacable]').html('<div class="amx-form__ty-message">Ваш отзыв отправлен</div>');
        }).fail(function (rsp) {
            console.log(rsp.responseJSON.error);
        });
    });
    $reqBtn.attr('disabled', 'disabled');

    /* news */
    var urlMonth = GetURLParameter('month'),
        urlYear = GetURLParameter('year'),
        newsElement = $('.news_table .mobile-select');
    if (newsElement.length) {}
});

$(document).ready(function () {
    window.successPageScriptsInit = function () {
        $('.js-success-detail__images-slider-slides').on('init', function (event, slick) {
            $('.js-success-detail__images-slider-page-count').text(slick.slideCount);
            if (slick.slideCount > 1) {
                $('.success-detail__images-slider-controls').show();
            }
        });

        $('.js-success-detail__images-slider-slides').on('beforeChange', function (event, slick, currentSlide, nextSlide) {
            $('.js-success-detail__images-slider-cur-page').text(nextSlide + 1);
        });

        $('.js-success-detail__images-slider-slides').slick();

        $('.js-success-detail__images-slider-prev').on('click', function (event) {
            event.preventDefault();
            $(this).closest('.success-detail__images-slider').find('.slick-prev').click();
        });

        $('.js-success-detail__images-slider-next').on('click', function (event) {
            event.preventDefault();
            $(this).closest('.success-detail__images-slider').find('.slick-next').click();
        });
    };

    successPageScriptsInit();

       //Замена кода слайдера
    // $('.js-main-page-success-slider').slick({
    //     arrows: false,
    //     dots: true,
    //     autoplay: true,
    //     autoplaySpeed: 4000,
    //     asNavFor: '.js-success-slider__note'
    // });

    // $('.js-success-slider__note').slick({
    //     arrows: false,
    //     swipe: false
    // });

    var initSliderMedia = function (index, elem) {
        index += 1;
  
        var gallery = $(elem);
        var sliderBig = gallery.find('.js-main-page-success-slider');
        var sliderSmall = gallery.find('.js-success-slider__note');
        var sliderBigInit = null;
        var sliderSmallInit = null;
  
  
        sliderBig.attr('data-slider-big', index);
        sliderBigInit = $('[data-slider-big =' + index + ']');
  
  
        sliderSmall.attr('data-slider-small', index);
        sliderSmallInit = $('[data-slider-small =' + index + ']');
  
        sliderBigInit.slick({
            arrows: false,
            dots: sliderBig.find('.success-slider__slide').length ==1 ? false : true,
            autoplay: false,
            autoplaySpeed: 4000,
            asNavFor: sliderSmallInit
        });
  
        sliderSmallInit.slick({
            arrows: false,
            swipe: false
        });
  
  
      }
  
    $('.success-slider__wrapper').each(function (index, elem) {
      initSliderMedia(index, elem)
  
    });
    //Замена кода слайдера
    if ($("#partneramContainer").length) {
        $(".site-page-header").hide();
    }
    $("#partneramList").on("change", function () {
        var target = $(this).val();
        $("#partneramListBtn").attr("href", target);
    });

    $(".js-pmSliderNav").on("mouseenter", function () {
        var index = $(this).index();
        $(".js-pmSliderImg").removeClass("is-current").eq(index).addClass("is-current");
        $(".js-pmSliderNavs .js-pmSliderNav").removeClass("is-current").eq(index).addClass("is-current");
        $(".js-pmSliderDots .js-pmSliderNav").removeClass("is-current").eq(index).addClass("is-current");
    });

    if ($("#partneramReviewsCount").length) {
        var reviewsCount = $(".js-partneramReviewsItem").length;
        var reviewCurrent = 1;
        if (reviewsCount === 1) {
            $("#partneramReviewsNav").hide();
        }
        $("#partneramReviewsCount").text(reviewsCount);

        $("#partneramReviewsNext").on("click", function () {
            reviewCurrent++;
            if (reviewCurrent > reviewsCount) {
                reviewCurrent = 1;
            }
            $("#partneramReviewsCurrent").text(reviewCurrent);
            $(".js-partneramReviewsItem").removeClass("is-current").eq(reviewCurrent - 1).addClass("is-current");
        });

        $("#partneramReviewsPrev").on("click", function () {
            reviewCurrent--;
            if (reviewCurrent < 1) {
                reviewCurrent = reviewsCount;
            }
            $("#partneramReviewsCurrent").text(reviewCurrent);
            $(".js-partneramReviewsItem").removeClass("is-current").eq(reviewCurrent - 1).addClass("is-current");
        });
    }
});
