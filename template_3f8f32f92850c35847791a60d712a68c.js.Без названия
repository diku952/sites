$(document).ready(function () {
    var parentMenu = $('.mobile-menu__sub-link.has-child');
    if (parentMenu.length) {
        $.each(parentMenu, function (i, e) {
            var index = $(this).data('parent-menu');
            var cld = $('[data-children-menu="'+index+'"]');
            if (cld.length) {
                $(this).on('click', function (event) {
                    event.preventDefault();
                    // console.log(cld);
                    cld.toggleClass('opened');
                });
            }
            // console.log(e);
        });
    }

    var device = navigator.userAgent.toLowerCase();
    var mobile = device.match(/(ipad)/);
    if (mobile) {
        var menu = $('.menu__link'),
            $preLastTouchStartAt = 0,
            $lastTouchStartAt = 0,
            $href = 0,
            $delay = 300;
        $.each(menu, function (i, e) {
            var _this = $(this);
            _this.on('click', function (event) {
                event.preventDefault();
                /*if (!$preLastTouchStartAt) {
                    $preLastTouchStartAt = new Date();
                    $href = $(this).attr('href');
                    return false;
                }
                $lastTouchStartAt = new Date();
                if ($href == $(this).attr('href') && $lastTouchStartAt !== $preLastTouchStartAt && $lastTouchStartAt - $preLastTouchStartAt < $delay) {
                    event.target.click();
                    window.location.href = $(this).attr('href');
                }
                $preLastTouchStartAt = $lastTouchStartAt;*/
            });
            _this.on('touchstart, touchend', function (event) {
                if (!$preLastTouchStartAt) {
                    $preLastTouchStartAt = new Date();
                    // event.preventDefault();
                    // $href = $(this).attr('href');
                    // return false;
                }
                $lastTouchStartAt = new Date();
                console.log($href == $(this).attr('href') && $lastTouchStartAt !== $preLastTouchStartAt && $lastTouchStartAt - $preLastTouchStartAt < $delay);
                if ($href == $(this).attr('href') && $lastTouchStartAt !== $preLastTouchStartAt && $lastTouchStartAt - $preLastTouchStartAt < $delay) {
                    event.preventDefault();
                    event.target.click();
                    window.location.href = $(this).attr('href');
                }
                $href = $(this).attr('href');
                $preLastTouchStartAt = $lastTouchStartAt;
            });
        });
    }
});
