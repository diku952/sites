    });
});

// Tall Textarea
//------------------------------------------------------------------------------

$(document).ready(function () {

    var $textarea = $('.js-textarea-tall');

    if ($textarea.length) {
        $textarea.each(function () {
            var $this = $(this);

            $this.on('change keyup keydown paste cut', 'textarea', function () {
                $(this).height(0).height(this.scrollHeight);
            }).find('textarea').change();
        });
    }
});

$(document).ready(function () {
    var $estimate = $('.js-estimate-list');

    if ($estimate.length) {
        $estimate.raty({
            hints: false,
            number: 10,
            path: '/local/templates/agima-main/history_application/images/',
            showCancel: false,
            showHalf: false,
            space: true,
            start: 3,
            starOff: 'star-off.png',
            starOn: 'star-on.png',
            click: function click(score) {
                $(this).attr('data-score', score);
            }
        });
    }
});

$(document).ready(function () {
    $(".f-select-multiple").filterSelector({
        header: !1,
        displaySelectedText: !0,
