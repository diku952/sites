; /* Start:"a:4:{s:4:"full";s:69:"/local/templates/agima-main/guideline/dist/js/main.js?155912365657158";s:6:"source";s:53:"/local/templates/agima-main/guideline/dist/js/main.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
var _typeof = typeof Symbol === "function" && typeof Symbol.iterator === "symbol" ? function (obj) { return typeof obj; } : function (obj) { return obj && typeof Symbol === "function" && obj.constructor === Symbol && obj !== Symbol.prototype ? "symbol" : typeof obj; };

function getParameterByName(name, url) {
    if (!url) url = window.location.href;
    name = name.replace(/[\[\]]/g, "\\$&");
    var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
        results = regex.exec(url);
    if (!results) return null;
    if (!results[2]) return '';
    return decodeURIComponent(results[2].replace(/\+/g, " "));
}

(function () {
    function widget(_class, selector, instance) {
        function init($c) {
            // записываем инстанс виджета в элемент
            $c.data(instance, new _class($c));
        }

        // записываем в глобальную область функцию init, чтобы можно было динамически инициализировать виджеты
        window['init' + instance] = init;

        $(selector).each(function () {
            init($(this));
        });
    }

    // еще одни Табы
    function Tabs($c) {
        var _this = this;

        function hideAllTabs() {
            var $buttons = $c.find('.js-n-tabs-button'),
                $items = $c.find('.js-n-tabs-item');

            $buttons.removeClass('_active');
            $items.removeClass('_active');
        }

        function showTab($button, $item) {
            $button.addClass('_active');
            $item.addClass('_active');
        }

        this.showTabBySelector = function (selector) {
            var $buttons = $c.find('.js-n-tabs-button');

            hideAllTabs();
            showTab($buttons.filter('[href="' + selector + '"]'), $(selector));
        };

        $c.on('click', '.js-n-tabs-button', function (e) {
            var $t = $(this),
                id = $t.attr('href');

            e.preventDefault();
            _this.showTabBySelector(id);
        });
    }

    // Поиск в классификации экспортных товаров
    function ClassificationSearch($c) {
        var _this = this,
            $resultsBlock = $c.find('.js-classification-search-result'),
            $container = $c.find('.js-classification-search-result-container');

        $c.on('submit', function (e) {
            e.preventDefault();

            $container.html('');

            var query = $c.find('.js-input').val();

            if (query.length) {
                request = $.get('/ajax/classificationSearch.php', { q: query });

                request.done(function (data) {
                    var $ids = [];
                    if (!data.STATUS) {
                        if (data.ITEMS.length) {
                            for (var i = 0; i < data.ITEMS.length; i++) {
                                if (!$ids[i]) {
                                    $container.append(data.ITEMS[i]);
                                    $ids[i] = true;
                                    console.log($ids);
                                }
                            }
                        } else {
                            $container.append('<b>Ничего не найдено.</b>');
                        }
                    } else {
                        $container.append(data.ERROR);
                    }
                    $resultsBlock.fadeIn();
                });
            } else {
                $resultsBlock.hide();
            }
        });
    }

    // тут вся инициализация
    $(function () {
        widget(Tabs, '.js-n-tabs', 'Tabs');
        widget(ClassificationSearch, '.js-classification-search', 'ClassificationSearch');
    });
})();

$(document).ready(function () {

    // F-select Init
    if ($('.f-select-multiple').length || $('.f-select').length) {

        $('.f-select-multiple').filterSelector({
            header: false, // button choose all
            displaySelectedText: true,
            filter: true,
            width: '100%',
            selectedList: 1,
            groupSelected: false,
            checkAllText: 'Выбрать все',
            noneSelectedText: function noneSelectedText() {
                return $(this).attr('placeholder');
            }
        });
        $('.f-select').filterSelector({
            multiple: false,
            header: false, // button choose all
            displaySelectedText: true,
            filter: true,
            width: '100%',
            selectedList: 1,
            groupSelected: false,
            checkAllText: 'Выбрать все',
            noneSelectedText: 'Выберите из списка'
        });
    }

    // Custom Scroll Init
    var $scroll = $('.js-scroll');
    $scroll.mCustomScrollbar({
        theme: 'rec'
    });

    // Checkbox Custom
    var $checkbox = $('.js-checkbox-custom'),
        $input = $('.js-input-custom');

    $checkbox.on('click', function () {
        $input.toggleClass('hidden');
    });
});

