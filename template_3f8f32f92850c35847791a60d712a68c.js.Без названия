    // }
    //
    // if (!localStorage.getItem('reviewsModalIsShown') === true) {
    //     modalOpenBtn.addClass('show');
    // }
    //
    // modalCloseBtn.click(() => {
    //     hideModal();
    // });
    //
    // modalWantBtn.click(() => {
    //     modalTitle.text('Спасибо!');
    //     modalText.text(
    //         'Благодарим Вас за внимание. На Вашу электронную почту будут высланы все необходимые инструкции'
    //     );
    //     modalDontBtn.remove();
