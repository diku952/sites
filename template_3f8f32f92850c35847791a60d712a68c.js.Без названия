// Toggle
//------------------------------------------------------------------------------

$(document).ready(function () {

    var $toggleLink = $('.js-toggle-link');

    $toggleLink.each(function () {

        $(this).on('click', function () {
            $(this).toggleClass('is-open');

            var open = $(this).hasClass('is-open'),
                openText = $(this).attr('data-open-text'),
                closeText = $(this).attr('data-close-text'),
                group = $(this).attr('data-group'),
                $block = $('.js-toggle-block'),
                $toggleBlock = $block.filter('[data-group="' + group + '"]');

            $toggleBlock.toggleClass('_hidden');
            open ? $(this).text(openText) : $(this).text(closeText);
        });
    });
});
/* End */
