// Manager Contacts

function managerContacts(opts) {
    var self = this;

    self.opts = opts;

    self.init = function () {

        self.opts.$button.on('click', function () {
            self.opts.$contacts.toggleClass('_hidden');
            $(this).toggleClass('_hidden');
        });
    };
}

$(document).ready(function () {

    var $manager = $('.js-app-manager');

    if ($manager.length) {
        $manager.each(function () {
            var $this = $(this);

            $this.data('managerContacts', new managerContacts({
                $root: $this,
                $button: $this.find('.js-app-manager-btn'),
                $contacts: $this.find('.js-app-manager-contacts')
            }));

            $this.data('managerContacts').init();
        });
    }
});
// Raty
// Country Item

var timeStamps;
function countryItem(opts) {
    var self = this;
    self.opts = opts;
    self.opts.$select = self.opts.$root.parents('.js-country-item').find('.js-country-select');
    self.init = function () {
        self.opts.$deleteCountry.on('click', function (e) {
            if (e.timeStamp === timeStamps) return false;
            timeStamps = e.timeStamp;
            var $country = this.closest('.js-country-selected');
            $country.remove();
            //т.к. ранее мы удалили из select выбранную страну - вернём её сейчас обратно
            self.opts.$select.prepend('<option value="' + $($country).find('input').val() + '">' + $country.innerText + '</option>');
            //отсортируем список option по алфавиту, т.к. мы добавили в начало ранее удалённую страну
            var sortedVals = $.makeArray(self.opts.$select.find('option')).sort(function (a, b) {
                var a = a.innerHTML.toUpperCase();
                var b = b.innerHTML.toUpperCase();
                return a < b ? -1 : a > b ? 1 : 0;
            });
            self.opts.$select.empty().html(sortedVals);
            //очистим значения select что бы отображался плейсхолдер
            self.opts.$select.val('');
        });
    };
}

function serviceItem(opts) {
    var self = this;

    self.opts = opts;

    self.init = function () {

        self.opts.$addButton.on('click', function () {
            self.addCountry();
            //удалим только что выбранный вариант, дабы не иметь возможности выбрать его повторно
            self.opts.$select.find('[value="' + self.opts.$select.val() + '"]').remove();
        });
        //если уже были ранее выбраны какие то страны - покажем их
        $(self.opts.$select).find('option[selected]').each(function () {
            var $name = $(this).val(),
                $block = self.opts.$addBlock,
                text = $(this).text();
            var idAccreditattion = $(this).parent().attr('id');
            idAccreditattion = idAccreditattion.replace(/[a-zA-Z_]*/, '');
            $item = '<div class="country-item__selected js-country-selected"' + '<span>' + text + '</span>' + '<input name="SELECTED_COUNTRIES[' + idAccreditattion + '][]" type="hidden" value="' + $name + '" />' + '<span class="country-item__selected-delete js-country-delete"></span>' + '</div>';
            $block.append($item);
            self.itemIn();
            //и удалим из спискта
            $(this).remove();
        });
    };

    self.addCountry = function () {

        var $name = self.opts.$select.val(),
            $block = self.opts.$addBlock,
            text;
        if ($name) {
            var idAccreditattion = self.opts.$select.parent().find('select').attr('id');
            text = self.opts.$select.find('option[value=' + $name + ']').eq(0).text();
            var $item = '<div class="country-item__selected js-country-selected"' + '<span>' + text + '</span>' + '<input name="SELECTED_COUNTRIES[' + idAccreditattion + '][]" type="hidden" value="' + $name + '" />' + '<span class="country-item__selected-delete js-country-delete"></span>' + '</div>';

            if ($name) {
                $block.append($item);
                self.itemIn();
            }
        }
    };

    self.itemIn = function () {
        var $countryItem = $('.js-country-selected');

        $countryItem.each(function () {
            var $this = $(this);

            $this.data('countryItem', new countryItem({
                $root: $this,
                $deleteCountry: $this.find('.js-country-delete')
            }));

            $this.data('countryItem').init();
        });
    };
}

$(document).ready(function () {
    var $serviceItem = $('.js-country-item');

    if ($serviceItem.length) {
        $serviceItem.each(function () {
            var $this = $(this);

            $this.data('serviceItem', new serviceItem({
                $root: $this,
                $addBlock: $this.find('.js-country-selected-block'),
                $select: $this.find('.js-country-select'),
                $addButton: $this.find('.js-country-add')
            }));

            $this.data('serviceItem').init();
        });
    }
});

// Input icon
//------------------------------------------------------------------------------

function iconInput(opts) {
    var self = this;

    self.opts = opts;

    self.init = function () {

        self.opts.$input.on('keyup', function () {
            self.iconView();
        });
    };

    self.iconView = function () {
        var $value = self.opts.$input.val(),
            $icon = self.opts.$icon;

        $value ? $icon.addClass('_active') : $icon.removeClass('_active');
    };
}

$(document).ready(function () {
    var $iconBlock = $('.js-icon-block');
    if ($iconBlock.length) {
        $iconBlock.each(function () {
            var $this = $(this);

            $this.data('iconBlock', new iconInput({
                $root: $this,
                $input: $this.find('.js-icon-input'),
                $icon: $this.find('.js-icon')
            }));

            $this.data('iconBlock').init();
        });
    }
});
// Left Menu Toggle

$(document).ready(function () {

    var $menuItem = $('.js-item-menu');
    $menuItem.on('click', function (e) {
        e.preventDefault();

        var $t = $(this);

        $t.toggleClass('is-open');
        $t.closest('li').toggleClass('is-open');
    });
});
$(document).ready(function () {

    // F-select Init
    $('.f-select-multiple').filterSelector({
        header: false, // button choose all
        displaySelectedText: true,
        filter: true,
        width: '100%',
        selectedList: 1,
        groupSelected: false,
        checkAllText: 'Выбрать все',
        noneSelectedText: function noneSelectedText() {
            return $(this).attr('placeholder');
        }
    });
    $('.f-select').filterSelector({
        multiple: false,
        header: false, // button choose all
        displaySelectedText: true,
        filter: true,
        width: '100%',
        selectedList: 1,
        groupSelected: false,
        checkAllText: 'Выбрать все',
        noneSelectedText: 'Выберите из списка'
    });

    // Custom Scroll Init
    var $scroll = $('.js-scroll');
    $scroll.mCustomScrollbar({
        theme: 'rec'
    });

    // Checkbox Custom
    var $checkbox = $('.js-checkbox-custom'),
        $input = $('.js-input-custom');

    $checkbox.on('click', function () {
        $input.toggleClass('hidden');
    });
});
// Scroll
function scrollBlock(opts) {
    var self = this;

    self.opts = opts;

    self.init = function () {

        self.opts.$input.on('change', function () {
            self.opts.$preview.toggleClass('_hidden');
            self.opts.$text.toggleClass('_hidden');
            var $open = self.opts.$input.prop('checked');
            $open ? self.opts.$buttonText.text('Свернуть') : self.opts.$buttonText.text('Подробнее');
        });
    };
}

function scrollBlockIn() {
    var $block = $('.js-scroll-block');

    if ($block.length) {
        $block.each(function () {
            var $this = $(this);

            $this.data('scrollBlock', new scrollBlock({
                $root: $this,
                $preview: $this.find('.js-scroll-preview'),
                $text: $this.find('.js-scroll-text'),
                $button: $this.find('.js-scroll-button'),
                $input: $this.find('.js-scroll-button-input'),
                $buttonText: $this.find('.js-scroll-button-text')
            }));

            $this.data('scrollBlock').init();
        });
    }
}

function scrollIn() {
    var $scroll = $('.js-scroll');
    $scroll.mCustomScrollbar({
        theme: 'rec'
    });
}

$(document).ready(function () {
    scrollIn();
    scrollBlockIn();
});
// Select2

function initSelect() {
    var $select = $('.js-select2');
    var mobileSelect2 = navigator.userAgent.toLowerCase().match(/(iphone|ipod|ipad|android|windows phone)/);

    if ($select.length && !mobileSelect2) {
        $select.select2({
            theme: 'rec',
            minimumResultsForSearch: Infinity,
            width: '100%',
            language: {
                noResults: function noResults() {
                    return "Не найдено";
                }
            }
        });

        $select.on('select2:open', function () {
            /*
             var $scrollList = $('.select2-results__options');
               $scrollList.mCustomScrollbar({
             theme: 'rec'
             });*/
        });
    }
    if ($select.length && mobileSelect2) {
        $select.addClass('mobile-select');
    }
}

$(document).ready(function () {
    initSelect();

    // Если при загрузки в селекте выберете направление ничего не выбрано, то подсветим его красным бордером
    if ($('#partnerListFilter').val() == null || $('#partnerListFilter').val() == '') {
        $('#partnerListFilter').siblings('.select2-container').addClass('error');
    }
});

!function () {
    function e(e, t, i) {
        function n(t) {
            t.data(i, new e(t));
        }

        window["init" + i] = n, $(t).each(function () {
            n($(this));
        });
    }

    function t(e) {
        function t() {
            var t = e.find(".js-n-tabs-button"),
                i = e.find(".js-n-tabs-item");
            t.removeClass("_active"), i.removeClass("_active");
        }

        function i(e, t) {
            e.addClass("_active"), t.addClass("_active");
        }

        var n = this;
        this.showTabBySelector = function (n) {
            var s = e.find(".js-n-tabs-button");
            t(), i(s.filter('[href="' + n + '"]'), $(n));
        }, e.on("click", ".js-n-tabs-button", function (e) {
            var t = $(this),
                i = t.attr("href");
            e.preventDefault(), n.showTabBySelector(i);
        });
    }

    function i(e) {
        /*var t = e.find(".js-classification-search-result"), i = e.find(".js-classification-search-result-container");
        e.on("submit", function (n) {
