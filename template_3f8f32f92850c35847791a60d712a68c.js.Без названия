// Tabs
//------------------------------------------------------------------------------

function sliderIn() {

    var $slider = $('.js-slider-owl');

    if ($slider.length) {
        $slider.each(function () {

            var $this = $(this),
                itemsCount = $this.attr('data-item-count');

            $this.owlCarousel({
                items: itemsCount,
                loop: false,
                margin: 30,
                dots: false,
                nav: true,
                navText: ["<span class='i-icon icon_arrow icon_arrow-left'></span>", "<span class='i-icon icon_arrow icon_arrow-right'></span>"]
            });
        });
    }
}

$(document).ready(function () {
    sliderIn();
});
