'use strict';

$.fn.handleDadataSuggestions = function () {
    return this.each(function (i, item) {
        var $root = $(item);
        var $fio = $root.find('[js-dadata-fio]');
        var $surname = $root.find('[js-dadata-surname], input[name="REGISTER[LAST_NAME]"]');
        var $name = $root.find('[js-dadata-name], input[name="REGISTER[NAME]"]');
        var $patronymic = $root.find('[js-dadata-patronymic], input[name="REGISTER[SECOND_NAME]"]');
        var $party = $root.find('[js-dadata-party], input[name="WORK_COMPANY"]');
        var $partyInfo = $root.find('[js-dadata-org-info]');
        var $ogrn = $root.find('[js-dadata-ogrn], input[name="REGISTER[UF_OGRN]"]');
        var $inn = $root.find('[js-dadata-ogrn], input[name="REGISTER[UF_INN]"]');
        var $address = $root.find('[js-dadata-ogrn], input[name="REGISTER[UF_ORG_ADDRESS]"]');

        var gender = 'UNKNOWN';

        $party.on('keyup', function () {
            $partyInfo.empty();
        });

        function init($element, type, parts) {
            $element.suggestions({
                token: "4215da902c9838352063c6b9daaca039928c14aa",
                type: type,
                triggerSelectOnSpace: false,
                hint: "",
                noCache: true,
                count: 20, // max
                params: {
                    parts: parts || []
                },
                onSearchStart: function onSearchStart(params) {
                    switch (type) {
                        case 'NAME':
                            params.gender = gender;
                            break;
                        case 'PARTY':
                            break;
                    }
                },
                onSelect: function onSelect(suggestion) {
                    switch (type) {
                        case 'NAME':
                            gender = suggestion.data.gender;
                            break;
                        case 'PARTY':
                            if ($ogrn.length && suggestion.data.ogrn) {
                                $ogrn.val(suggestion.data.ogrn);
                                $ogrn.attr('readonly', true);
                            }
                            if ($inn.length && suggestion.data.inn) {
                                $inn.val(suggestion.data.inn);
                                $inn.attr('readonly', true);
                            }
                            if ($address.length && suggestion.data.address) {
                                $address.val(suggestion.data.address.value);
                            }
                            if ($partyInfo.length) {
                                var info = [];
                                if (suggestion.data.ogrn) {
                                    info.push(suggestion.data.ogrn);
                                }
                                if (suggestion.data.inn) {
                                    // info.push(suggestion.data.inn);
                                }
                                if (suggestion.data.address) {
                                    info.push(suggestion.data.address.value);
                                }
                                if (info.length > 0) {
                                    $partyInfo.html(info.join(' &nbsp; '));
                                }
                            }

                            $element.trigger('on-dd-select', this);

                            break;
                    }
                }
            });
        }

        if ($fio.length) {
            init($fio, 'NAME');
        }
        if ($surname.length) {
            init($surname, 'NAME', ['SURNAME']);
        }
        if ($name.length) {
            init($name, 'NAME', ['NAME']);
        }
        if ($patronymic.length) {
            init($patronymic, 'NAME', ['PATRONYMIC']);
        }
        if ($party.length) {
            init($party, 'PARTY');
        }
    });
};

(function () {
    var Layer;
    var $document, $window, $body, $bodyWrapper, $overlay, overlayAllowed;

    var initialized,
        opened = [],
        bodyScroll,
        storage = {};

    var overlayTimer = void 0;

    function initialize() {
        if (initialized) return;
        initialized = true;

        $document = $(document);
        $window = $(window);
        $body = $('body');
        $bodyWrapper = $('.body__wrapper');

        overlayAllowed = true;
        if (overlayAllowed) {
            $overlay = $('<div class="body__overlay"></div>').appendTo($body);

            $overlay.on('click', function (e) {
                e.preventDefault();
                if (opened && typeof opened[opened.length - 1] !== 'undefined') {
                    opened[opened.length - 1].close();
                }
            });
        }

        $document.on('customOpen', '.js-custom-open', function (e, id, callback){
            e.preventDefault();
            e.stopPropagation();
            var callbackResult = callback();
            if(callbackResult){
                open(id);
            }
        });

	    $document.on('click', '[data-js-open-layer="auth"]', function (e) {
		    e.preventDefault();
		    e.stopImmediatePropagation();
		    BX.showWait();
            /**
             * 32693
             */
            var parentName = $(this).parent().data('name'),
                elementName = $(this).data('name');

            if (elementName && elementName !== 'undefined') {
                window.location.hash = elementName;
            } else if (parentName && parentName !== 'undefined') {
                window.location.hash = parentName;
            }

            var thisId = $(this).attr('data-js-open-layer');
		    if ($('[data-id="'+thisId+'"]').length) open(thisId);
		    else {
			    $.ajax({
				    url: '/ajax/open-layer-'+thisId+'.php',
				    type: 'POST',
				    data: {},
				    success: function (e) {
					    $('.page_container').append(e);
					    setTimeout(function(){
						    initRegistrForm();
						},150);
					    open(thisId);
					    BX.closeWait();
				    },
				    error: function (e) {
					    console.log(e);
				    }
			    })
		    }
	    });
	    $document.on('click', '[data-js-open-layer="auth-agreement"]', function (e) {
		    e.preventDefault();
		    e.stopImmediatePropagation();
		    BX.showWait();
		    var thisId = $(this).attr('data-js-open-layer');
		    if ($('[data-id="'+thisId+'"]').length) open(thisId);
		    else {
			    $.ajax({
				    url: '/ajax/agreement.php',
				    type: 'POST',
				    data: {},
				    success: function (e) {
					    $('.page_container').append(e);
					    setTimeout(function(){
						    initRegistrForm();
					    },150);
					    open(thisId);
					    BX.closeWait();
				    },
				    error: function (e) {
					    console.log(e);
				    }
			    })
		    }
	    });
	    $document.on('click', '[data-js-open-layer="auth-personal-data"]', function (e) {
		    e.preventDefault();
		    e.stopImmediatePropagation();
		    BX.showWait();
		    var thisId = $(this).attr('data-js-open-layer');
		    if ($('[data-id="'+thisId+'"]').length) open(thisId);
		    else {
			    $.ajax({
				    url: '/ajax/personal_data.php',
				    type: 'POST',
				    data: {},
				    success: function (e) {
					    $('.page_container').append(e);
					    setTimeout(function(){
						    initRegistrForm();
					    },150);
					    open(thisId);
					    BX.closeWait();
				    },
				    error: function (e) {
					    console.log(e);
				    }
			    })
		    }
	    });
	    $document.on('click', '[data-js-open-layer="request"], [data-js-open-layer="request2"]', function (e) {
		    e.preventDefault();
		    e.stopImmediatePropagation();
		    BX.showWait();
		    var thisId = $(this).attr('data-js-open-layer');
            function afterAjaxForm() {
	            setTimeout(function(){
		            $('.partner-form')[0].reset();

                    // Делаем кнопки lavel загрузки файла с уникальным for
                    // баг с дублем формы на странице партнеры - детальная
                    // смог решить только так
                    if ($('.partner-form').eq(1)) {
                        $('.partner-form').eq(1).find('.js-attach').attr('for', 'message-attach-file-2');
                        $('.partner-form').eq(1).find('#message-attach-file').attr('id', 'message-attach-file-2');
                    }

		            $('.js-attach-delete').click();
		            $('*[data-step-select]').each(function () {
			            var id = $(this).attr('data-step-select');
			            if (Array.isArray($(this).val())) {
				            var arr = $(this).val();
				            $('*[data-step-select-copy=0]').trigger('change');
				            for (var i = 0; i < arr.length; i++) {
					            $('.js-partner-spec-item[data-number]').last().find('*[data-step-select-copy]').val(arr[i]).trigger('change');
				            }
			            } else $('*[data-step-select-copy=' + id + ']').val($(this).val()).trigger('change');
		            });
		            fillRequestForm('#partnerListFilter', 'chosen-direction', 'DIRECTION', thisId);
		            fillRequestForm('#partnerListFilterFor', 'chosen-services', 'SERVICE[]', thisId);
	            },150);
            }
		    function afterAjaxFormHandler() {
			    setTimeout(function(){
				    partnershipForm();
			    },150);
		    }
		    if ( Layer.has(thisId) ) {
		        open(thisId);
			    afterAjaxForm();
			    BX.closeWait();
		    }
		    else {
			    $.ajax({
				    url: '/ajax/open-layer-'+(thisId === 'request2'? 'request': thisId)+'.php',
				    type: 'POST',
				    data: {},
				    success: function (e) {
					    $('.g-section:first').append(e);
					    open(thisId);
					    afterAjaxForm();
					    afterAjaxFormHandler();
					    BX.closeWait();
				    },
				    error: function (e) {
					    console.log(e);
				    }
			    })
		    }
	    });

        $document.on('click', '.add_user_event', function (e) {
            e.preventDefault();
            e.stopImmediatePropagation();

            var eID = $(this).attr('data-evt-id');
            var eNAME = $(this).attr('data-evt-name');
            var eStart = $(this).attr('data-evt-start');
            var eEnd = $(this).attr('data-evt-end');
            var eCity = $(this).attr('data-evt-city');
            var eCountry = $(this).attr('data-evt-country');
            var thisId = $(this).attr('data-js-open-layer-statics');
            var addUserEvent = new XMLHttpRequest();

            if ($('[data-id="event-mess"]').length >= 0) {
                $('[data-id="event-mess"]').remove();
            }

            addUserEvent.open('GET', '/ajax/user_events.php?event_id=' + eID + '&event_name=' + eNAME + '&start=' + eStart + '&end=' + eEnd + '&city=' + eCity + '&country=' + eCountry + '&fn=add_comp', true);
            addUserEvent.setRequestHeader('X-Requested-With', 'XMLHttpRequest');

            addUserEvent.onload = function () {
                if (addUserEvent.status >= 200 && addUserEvent.status < 400) {
                    var data = addUserEvent.responseText;

                    $('.page_container').append(data);

                    if (storage[thisId]) {
                        delete storage[thisId];
                    }

                    open(thisId);

                    setTimeout(function (e) {
                        var $lay = $('[data-id="event-mess"]');


                        $lay.find('.close_comp').on('click', function (e) {
                            e.preventDefault();
                            $lay.find('.layer__close').click();
                        });

                        $lay.find('.add_comp').on('click', function (e) {
                            e.preventDefault();

                            $lay.find('.l-content--show').hide();
                            $lay.find('.l-content--hide').show();
                        });

                        $lay.find('form').on('submit', function (e) {
                            e.preventDefault();

                            var formBlock = this;
                            var form = new FormData(this);
                            var addUserEventComp = new XMLHttpRequest();

                            addUserEventComp.open('POST', '/ajax/user_events.php?event_id=' + eID + '&event_name=' + eNAME + '&start=' + eStart + '&end=' + eEnd + '&city=' + eCity + '&country=' + eCountry + '&fn=add_comp_secc', true);
                            addUserEventComp.setRequestHeader('X-Requested-With', 'XMLHttpRequest');

                            addUserEventComp.onload = function () {
                                if (addUserEvent.status >= 200 && addUserEvent.status < 400) {
                                    $lay.find('.layer__body').html(addUserEventComp.responseText);
                                }
                            }

                            addUserEventComp.send(form);
                        });
                    }, 10)

                } else {
                    console.log(data);
                }
            };
            addUserEvent.send();
        })


	    $document.on('click', '[data-js-open-layer]', function (e) {
		    e.preventDefault();
		    e.stopPropagation();
		    open($(this).attr('data-js-open-layer'));
	    });

        $document.on('click', '[data-js-toggle-layer]', function (e) {
            e.preventDefault();
            e.stopPropagation();
            toggle($(this).attr('data-js-toggle-layer'));
        });
    }

    function hasLayer(id) {
        return !!storage[id];
    }

    function findLayer(id) {
        var layer = storage[id];

        if (!layer) {
			layer = new Layer($('.layer[data-id="' + id + '"]')[0]);
            storage[layer.id] = layer;
        }
        return layer;
    }

    function open(id, data) {
        findLayer(id).open(data);
    }

    function toggle(id) {
        var layer = findLayer(id);
        if (layer.opened) {
            layer.close();
        } else {
            layer.open();
        }
    }

    function showOverlay() {
        if (overlayAllowed) {
            clearTimeout(overlayTimer);
            $overlay.addClass('is-showed');
            $overlay[0].offsetWidth;
            $overlay.addClass('is-opened');
        }
    }

    function hideOverlay() {
        if (overlayAllowed) {
            $overlay.removeClass('is-opened');
            clearTimeout(overlayTimer);
            overlayTimer = setTimeout(function () {
                $overlay.removeClass('is-showed');
            }, 700);
        }
    }

    function lockBody() {
        $body.addClass('body_lock');
        $bodyWrapper.css('top', -(bodyScroll = $document.scrollTop())).addClass('is-locked');
    }

    function unlockBody() {
        $body.removeClass('body_lock');
        $bodyWrapper.css('top', 'auto').removeClass('is-locked');
        $document.scrollTop(bodyScroll);
    }

    function hideBody() {
        $bodyWrapper.addClass('is-hide');
    }

    function showBody() {
        $bodyWrapper.removeClass('is-hide');
    }

    var $layer;

    if ($layer) {
        console.log($layer);

    }

    Layer = function Layer($layer, options) {
        options = $.extend({
            scrollToTop: true,
            aside: true
        }, options || {});

        $.extend(this, {
            options: options,
            id: null,
            initialized: false,
            opened: false,
            $origin: null,
            $layer: null
        });

        this.initialize($layer);
    };

    $.extend(Layer.prototype, {
        initialize: function initialize($layer) {
            if (this.initialized) return;
            this.initialized = true;
            $layer = $($layer);
            if (!$layer.length) {
                throw new Error('Layer not found');
            }

            this.$origin = this.$layer = $layer.detach();
            this.id = $layer.data('id');

            $layer.addClass('is-initialized');

            //fix for iOS
            var closeButtons = $layer.find('.layer__close, .layer__back, [js-close-layer], .jqmClose');
            var $thisElement = this;
            $.each(closeButtons, function (e) {
                var _this = $(this);
                _this.on('click', function (e) {
                    if (!$(e.currentTarget).attr('href')) {
                        e.preventDefault();
                        $thisElement.close($(e.currentTarget).data('close-delay') || 0);
                    }
                });
            });

            $layer.on('click', '.layer__close, .layer__back, [js-close-layer], .jqmClose', function (e) {
                if (!$(e.currentTarget).attr('href')) {
                    e.preventDefault();
                    this.close($(e.currentTarget).data('close-delay') || 0);
                }
            }.bind(this));

            $layer.find('.layer-chooser').each(function () {
                var $root = $(this);
                var $items = $root.find('.layer-chooser__item');
                $items.on('click', function (e) {

                    e.preventDefault();
                    var dataLink = $(this).attr('data-link') || '';

                    $layer.removeClass('reg-container');
                    if(dataLink === '.auth-registration') {
                        $layer.addClass('reg-container');
                    }

                    var popupTitle = $('.js-auth-title').attr('data-' + dataLink.substr(1)) || '';
                    if(popupTitle.length > 0){
                        $('.js-auth-title').html(popupTitle);
                    }

                    $items.each(function () {
                        $layer.find($(this).data('link')).hide();
                    }).removeClass('is-active');

                    $layer.find($(this).addClass('is-active').data('link')).show();
                }).filter('.is-active').click();
            });

            $layer.on('layer-close', function () {
                this.close();
            }.bind(this));
        },
        open: function open(data) {

            if (this.options.clone) {
                this.$layer = this.$origin.clone(true, true);
            }

            var $layer = this.$layer,
                $header = $layer.find('.layer-header'),
                top = opened[opened.length - 1];
            $layer.css('top', 0);

            if (this.options.beforeOpen) {
                this.options.beforeOpen.call(this, data, this.$layer);
            }

            if (opened.length) {
                top.scrollTop = $window.scrollTop();
                top.$layer.css('top', -top.scrollTop);
                opened.forEach(function (layer) {
                    layer.$layer.addClass('is-faded');
                });
            } else {
                lockBody();
            }

            if (!opened.length) {
                if (this.options.aside && overlayAllowed) {
                    showOverlay();
                    $overlay.removeClass('is-complete');
                } else {
                    hideOverlay();
                    hideBody();
                }
            }

            opened.push(this);
            this.opened = true;

            if (this.options.headline) {
                this.$layer.prepend($header = $('.headline').clone());
            }

            setTimeout(function () {
                $body.append($layer.addClass('is-animating'));

                if (this.options.aside) {
                    $layer.addClass('is-aside');
                }

                $layer[0].offsetHeight;
                $layer.addClass('is-opened');

                $layer.one('transitionend webkitTransitionEnd oTransitionEnd otransitionend MSTransitionEnd', function (e) {
                    e.stopPropagation();
                    e.stopImmediatePropagation();
                    $layer.removeClass('is-animating');

                    if (this.options.aside && overlayAllowed) {
                        $overlay.addClass('is-complete');
                    }
                }.bind(this));

                if ($header.length) {
                    $layer.css('padding-top', $header.outerHeight());
                }

                $document.scrollTop(0);

                $window.trigger('updateToTop', { $layer: $layer });
                $window.trigger('updateFixedButtons');

                if ($.fn.inputmask) {
                    $layer.find('[data-inputmask]').inputmask();
                }

                if (this.options.afterOpen) {
                    this.options.afterOpen.call(this, data, this.$layer);
                }
            }.bind(this), 10);
        },

        close: function close(delay) {
            opened = opened.filter(function (layer) {
                return layer !== this;
            }, this);
            var $layer = this.$layer;
            $layer.css('top', -$window.scrollTop());
            $layer.addClass('is-closing');

            setTimeout(function () {
                var top = opened[opened.length - 1];

                if (top && top.$layer.hasClass('is-faded')) {
                    top.$layer.css('top', 0).removeClass('is-faded');
                    $window.scrollTop(top.scrollTop);
                }

                $layer.addClass('is-animating').removeClass('is-opened');

                if (!opened.length || top.options.aside && overlayAllowed) {
                    showBody();
                }

                if (!opened.length) {
                    if (this.options.aside && overlayAllowed) {
                        hideOverlay();
                    } else if (top && top.options.aside && overlayAllowed) {
                        showOverlay();
                    }
                }

                setTimeout(function () {
                    $layer.removeClass('is-animating').removeClass('is-closing').detach();
                    this.opened = false;

                    if (!opened.length) {
                        unlockBody();
                    }

                    if (this.options.headline) {
                        $layer.find('.headline').remove();
                    }

                    $window.trigger('updateToTop', { $layer: opened.length ? opened[opened.length - 1].$layer : false });
                    $window.trigger('updateFixedButtons');

                    if (this.options.afterClose) {
                        this.options.afterClose.call(this, $layer);
                    }
                }.bind(this), 300);
            }.bind(this), delay || 0);
        }
    });

    $.extend(Layer, {
        open: open,
        toggle: toggle,
        has: hasLayer
    });

    $(function () {
        initialize();
    });

    window.Layer = Layer;

    $.fn.layer = function (options) {
        setTimeout(function () {
            this.each(function () {
                var layer = new Layer(this, options);
                storage[layer.id] = layer;
            });
        }.bind(this), 10);
    };
})();

$.fn.reflow = function () {
    this.each(function () {
        this.offsetWidth;
    });
    return this;
};

window.selectbox = function (context) {

    $('.selectbox', context).each(function () {
        var $select = $(this),
            $value = $select.find('.selectbox__value'),
            $optionsContainer = $select.find('.selectbox__options'),
            $options = $select.find('.selectbox__option'),
            $inputs = $options.find('input[type="checkbox"], input[type="radio"]');

        var IS_SEARCHABLE = !!$select.data('search');
        var isAjax = !!$select.data('is-ajax');
        var $search = void 0;
        var $searchInput = void 0;
        var $searchEmpty = void 0;

        if (IS_SEARCHABLE) {
            $search = $('<div class="selectbox__search"><input type="text"></div>');
            $searchInput = $search.find('input');
            $searchEmpty = $('<div class="selectbox__search-empty">Нет подходящих вариантов</div>');
            $optionsContainer.prepend($search);
            $optionsContainer.append($searchEmpty);

            if (!!$select.data('input-placeholder')) {
                $searchInput.attr('placeholder', $select.data('input-placeholder'));
            }

            $searchInput.on('input', function (e) {
                e.preventDefault();
                var value = (e.target.value || '').trim().toLowerCase();
                var empty = true;
                if (value.length > 0) {
                    $options.each(function (i, item) {
                        var $item = $(item);
                        if (value === ('' + $item.find($inputs).data('label') || '').substring(0, value.length).toLocaleLowerCase()) {
                            $item.show();
                            empty = false;
                        } else {
                            $item.hide();
                        }
                    });
                } else {
                    $options.show();
                    empty = false;
                }
                if (empty) {
                    $searchEmpty.show();
                } else {
                    $searchEmpty.hide();
                }
            });

            $searchInput.on('change', function (e) {
                if (isAjax) {

                } else {
                    e.preventDefault();
                    e.stopPropagation();
                    e.stopImmediatePropagation();
                }
            });
        }

        $optionsContainer.on('click touchstart', function (e) {
            e.stopPropagation();
            e.stopImmediatePropagation();
        });

        $value.on('click', function (e) {
            e.preventDefault();
            if (!$select.hasClass('is-opened')) {
                $select.addClass('is-opened');

                if (IS_SEARCHABLE && !$select.data('input-placeholder')) {
                    $searchInput.focus();
                }

                setTimeout(function () {
                    $(document).one('click touchstart', function () {
                        $select.removeClass('is-opened');
                    });
                }, 10);
            }
        });

        $inputs.on('change', function () {
            if ('radio' == this.getAttribute('type')) {
                $select.click();
            }
            $select.triggerHandler('render');
        });

        $select.on('render', function (e) {
            e.stopPropagation();

            $select.triggerHandler('beforeRender');

            var values = [];
            var $checked = $inputs.filter(':checked').each(function () {
                values.push($(this).data('label'));
            });

            if ($select.data('placeholder') && (!$checked.length || 1 === $checked.length && ($checked.val() == $select.data('default') || ''))) {
                $value.text($select.data('placeholder'));
            } else {
                $value.text(values.join(', '));
            }
        });
        $select.triggerHandler('render');
    });
};

$(function () {
    selectbox();
});

function initRegistrForm() {

    var $registration = $('.auth-registration');

    var widgetIdReg = null;

    var step1 = $('#reg-form-step-1');
    var step2 = $('#reg-form-step-2');

    if (!$registration.length) {
        return;
    }

    var $login = $('.auth-login');
    var $forgot = $('.auth-forgot');
    var $registrationForm = $registration.find('form');
    var $loginForm = $login.find('form');
    var $forgotForm = $forgot.find('form');

    // var $personalRegBlock = $registration.find('.personal_label-container');
    // var $bottomWrapper = $registration.find('.bottom_wrapper');
    // var $cabinetPreview = $registration.find('.cabinet-preview');

    $registrationForm.find('input[name="REGISTER[UF_OGRN]"]').mask('9999999999999');
    $registrationForm.find('input[name="REGISTER[UF_OGRN]"]').addClass('ogrn');
    $registrationForm.find('input[name="REGISTER[PERSONAL_PHONE]"]').addClass('phone');
    $registrationForm.find('input[name="REGISTER[PASSWORD]"]').addClass('password');
    $registrationForm.find('input[name="REGISTER[CONFIRM_PASSWORD]"]').addClass('confirm_password');
    $registrationForm.validate({
        highlight: function highlight(element) {
            $(element).addClass('error');
            if ($(element).hasClass('checkbox')) {
                $('.user-agree label.error').length ? $('.checkbox-border').addClass('error') : '';
            }
        },
        unhighlight: function unhighlight(element) {
            $(element).removeClass('error').addClass('success');
        },
        submitHandler: function submitHandler(form) {
            if ($registrationForm.valid()) {
                if ($registrationForm.find('button[type="submit"]').data('sitekey')) {
                    grecaptcha.execute(widgetIdReg);
                }
                $registrationForm.stop().animate({ opacity: .5 }, 600);
                $registrationForm.find('button[type="submit"]').attr('disabled', 'disabled');
                $registrationForm.find('.form__error').empty();
                $.ajax({
                    type: 'post',
                    url: $registrationForm.attr('action'),
                    dataType: 'html',
                    data: $registrationForm.serialize(),
                    success: function success(response) {
                        var $response = $(response);
                        var $error = $response.find('.errortext');
                        var $success = $response.find('.auth-registration-complete');
                        if ($error.length) {
                            if ($registrationForm.find('button[type="submit"]').data('sitekey')) {
                                grecaptcha.reset(widgetIdReg);
                            }
                            $registrationForm.find('.form__error').html($error.html());
                        }
                        if ($success.length) {
                            $registrationForm.empty().append($success);
                        }
                    },
                    complete: function complete() {
                        $registrationForm.stop().animate({ opacity: 1 }, 300);
                        $registrationForm.find('button[type="submit"]').removeAttr('disabled');
                    }
                });
            } else {
                grecaptcha.reset(widgetIdReg);
            }
        },
        errorPlacement: function errorPlacement(error, element) {
            var $section = $(element).closest('.form__section');
            var $error = $section.find('.form__section-item_error');
            $error.append(error);
        }
    });

    /*$registrationForm.find(".password").rules("add", {
        messages: {
            regexp: BX.message('JS_PASS')
        }
    });*/

    $(document).on('click', '.user_reg__change-data', function () {
        step1.show();
        step2.hide();
        $('#WORK_COMPANY').val('');
    });

    $registration.on('on-dd-select', function (e) {
        step1.hide();
        step2.show();

        if (document.getElementById('reg_btn')) {
            if ($('#reg_btn').length && $('#reg_btn').data('sitekey')) {
                widgetIdReg = grecaptcha.render('reg_btn', {
                    'sitekey': $('#reg_btn').data('sitekey'),
                    'callback': function (token) {
                        if (rInput.val() === '') {
                            rInput.val(token);
                        }

                        if (!$registrationForm.valid()) {
                            grecaptcha.reset(widgetIdReg);
                        } else {
                            grecaptcha.execute(widgetIdReg);
                            $registrationForm.trigger('submit');
                        }
                    }
                });
            }
        }

        var rInput = $("#gRecaptchaResponse");

        $('.layer-bg_auth').addClass('reg-container');

        var cName = $(e.target).val();
        if(cName.length) {
            $registrationForm.find('input[name="REGISTER[WORK_COMPANY]"]').val(cName).attr('readonly', 'readonly');
        }
    });

    var rfEmail = $registrationForm.find(".e-mail"),
        rfCheckbox = $registrationForm.find(".checkbox");

    if(rfEmail.length) {
        $registrationForm.find(".e-mail").rules("add", {
            messages: {
                regexp: BX.message('JS_MAIL')
            }
        });
    }

    if(rfCheckbox.length) {
        $registrationForm.find(".checkbox").rules("add", {
            messages: {
                required: BX.message('JS_REQUIRED_CHECKBOX')
            }
        });
    }

    if (arScorpOptions['THEME']['PHONE_MASK'].length) {
        var base_mask = arScorpOptions['THEME']['PHONE_MASK'].replace(/(\d)/g, '_');
        $('form[name="regform"] input.phone').inputmask('mask', { 'mask': arScorpOptions['THEME']['PHONE_MASK'] }).blur(function () {
            if ($(this).val() == base_mask || $(this).val() == '') {
                if ($(this).hasClass('required')) {
                    $(this).parent().find('div.error').html(BX.message('JS_REQUIRED'));
                }
            }
        });
    }
    //
    // $('#agreement').jqm({ajax: '/ajax/agreement.php', trigger: "#agreement-show"});
    // $('#personal-data').jqm({ajax: '/ajax/personal_data.php', trigger: "#personal-data-show"});


    $loginForm.on('submit', function (e) {
        e.preventDefault();
        $loginForm.stop().animate({ opacity: .5 }, 600);
        $loginForm.find('button[type="submit"]').attr('disabled', 'disabled');
        $loginForm.find('.form__error').empty();
        $.ajax({
            type: 'post',
            url: $loginForm.attr('action'),
            dataType: 'html',
            data: $loginForm.serialize(),
            success: function success(response) {
                var $response = $(response);
                var $error = $response.find('.text-danger');
                var $captcha = $response.find('.captcha__div');
                var $success = $response.find('.auth-auth-complete');
                var $redirectUrl = $response.find('.auth-auth-complete').attr('data-backurl') || '';
                if ($error.length) {
                    $loginForm.find('.form__error').html($error.html());
                }
                if ($captcha.length) {
                    $loginForm.find('.form__captcha').html($captcha.html());
                }
                if ($success.length) {
                    $loginForm.closest('.layer').triggerHandler('layer-close');
                    document.location.reload();
                }
                if ($redirectUrl.length > 0) {
                    $loginForm.closest('.layer').triggerHandler('layer-close');
                    document.location.replace($redirectUrl);
                }
            },
            complete: function complete() {
                $loginForm.stop().animate({ opacity: 1 }, 300);
                $loginForm.find('button[type="submit"]').removeAttr('disabled');
            }
        });
    });

    $forgotForm.on('submit', function (e) {
        e.preventDefault();
        var submit = $forgotForm.find('button[type="submit"]');
        $forgotForm.stop().animate({ opacity: .5 }, 600);
        submit.attr('disabled', 'disabled');
        $forgotForm.find('.form__error').empty();
        $.ajax({
            type: 'post',
            url: $forgotForm.attr('action'),
            dataType: 'html',
            data: $forgotForm.serialize(),
            success: function success(response) {
                var $response = $(response);
                var $error = $response.find('.text-danger');
                var $success = $response.find('.auth-forgot-success');
                if ($error.length) {
                    $forgotForm.find('.form__error').html($error.html());
                    submit.removeAttr('disabled');
                }
                if ($success.length) {
                    $forgotForm.find('.form__success-inline').html($success.html());
                }
                submit.css('background-color', '#CDD2DB').css('cursor', 'default');
            },
            complete: function complete() {
                $forgotForm.stop().animate({ opacity: 1 }, 300);
            }
        });
    });

    $registration.handleDadataSuggestions();

	$(document).on('click', '.auth__switch-link-restore', function(e) {
		e.preventDefault();
        $forgot.slideDown(300);
        $('.layer__header').hide('slow');
        $login.slideUp(300);
	});

    $login.find('.auth__switch-link').on('click', function (e) {
        e.preventDefault();
        $forgot.slideDown(300);
        $('.layer__header').hide('slow');
        $login.slideUp(300);
    });

    $forgot.find('.auth__switch-link').on('click', function (e) {
        e.preventDefault();
        $login.slideDown(300);
        $('.layer__header').show('slow');
        $('.form__error').text('');
        $forgot.slideUp(300);
    });

    $registration.on('click', '.auth-registration-complete__auth', function (e) {
        e.preventDefault();

        $registration.closest('.layer').find('.layer-chooser__item[data-link=".auth-auth"]').click();
    });
};

$(function () {
    $('.page-main').on('click', '.contacts-department__map-toggler', function (e) {
        e.preventDefault();

        var $map = $(this).closest(".contacts-department").find(".contacts-department__map");

        if ($(this).hasClass("is-active")) {
            $map.slideUp();
        } else {
            $map.slideDown();
        }

        $(this).toggleClass("is-active");
    });
});

/**
 * Created by developer1 on 22.05.2017.
 */
//Download files

window.downloadFile = function (sUrl) {

    //iOS devices do not support downloading. We have to inform user about this.
    if (/(iP)/g.test(navigator.userAgent)) {
        alert('Your device does not support files downloading. Please try again in desktop browser.');
        return false;
    }

    //If in Chrome or Safari - download via virtual link click
    if (window.downloadFile.isChrome || window.downloadFile.isSafari || window.downloadFile.isFirefox) {
        //Creating new link node.
        var link = document.createElement('a');
        link.href = sUrl;

        if (link.download !== undefined) {
            //Set HTML5 download attribute. This will prevent file from opening if supported.
            var fileName = sUrl.substring(sUrl.lastIndexOf('/') + 1, sUrl.length);
            link.download = fileName;
        }

        //Dispatching click event.
        if (document.createEvent) {
            var e = document.createEvent('MouseEvents');
            e.initEvent('click', true, true);
            link.dispatchEvent(e);
            return true;
        }
    }

    // Force file download (whether supported by server).
    if (sUrl.indexOf('?') === -1) {
        sUrl += '?download';
    }

    window.open(sUrl, '_self');
    return true;
};

window.downloadFile.isChrome = navigator.userAgent.toLowerCase().indexOf('chrome') > -1;
window.downloadFile.isSafari = navigator.userAgent.toLowerCase().indexOf('safari') > -1;
window.downloadFile.isFirefox = navigator.userAgent.toLowerCase().indexOf('firefox') > -1;

$(function () {
    var $dropdownList = $('.dropdown');
    if ($dropdownList.length) {
        $('.dropdown').each(function () {
            var $dropdown = $(this);
            var $label = $dropdown.find('.dropdown__label');
            $label.on('click', function (e) {
                e.preventDefault();
	            $dropdown.toggleClass('is-opened')
            });
        });
	    $('.menu__mobile-holder').on('click', function (e) {
		    if (!$dropdownList.is(e.target) && $dropdownList.has(e.target).length === 0) {
			    $dropdownList.removeClass('is-opened');
		    }
	    });
    }
});

$(function () {
    var $radio = $('[js-bk-radio]'),
        $radioSelect = $('[js-bk-radio-select]'),
        $checkbox = $('[js-bk-checkbox]'),
        $checkboxSelect = $('[js-bk-checkbox-select]'),
        $textarea = $('[js-bk-textarea]'),
        $input = $('[js-bk-input]');

    $radio.on('change', function () {
        $(this).closest('.bk-quiz__section-main').prev().removeClass('bk-quiz__section-title_error');
    });

    $checkbox.on('change', function () {
        $(this).closest('.bk-quiz__section-main').prev().removeClass('bk-quiz__section-title_error');
    });

    $textarea.on('blur', function () {
        if ($(this).val() !== '') {
            $(this).closest('.bk-textarea').removeClass('bk-textarea_error');
            $(this).closest('.bk-textarea').closest('.bk-quiz__section-main').prev().removeClass('bk-quiz__section-title_error');
        }
    });

    $textarea.on('keyup', function () {
        if ($(this).val() !== '') {
            $(this).closest('.bk-textarea').removeClass('bk-textarea_error');
            $(this).closest('.bk-textarea').closest('.bk-quiz__section-main').prev().removeClass('bk-quiz__section-title_error');
        }
    });

    $radioSelect.on('change', function () {
        $(this).closest('.selectbox').removeClass('selectbox_error');
        $(this).closest('.selectbox').closest('.bk-quiz__section-main').prev().removeClass('bk-quiz__section-title_error');
    });

    $checkboxSelect.on('change', function () {
        $(this).closest('.selectbox').removeClass('selectbox_error');
        $(this).closest('.selectbox').closest('.bk-quiz__section-main').prev().removeClass('bk-quiz__section-title_error');
    });

    $input.on('blur', function () {
        if ($(this).val()) {
            $(this).closest('.bk-input').removeClass('bk-input_error');
            $(this).closest('.bk-input').closest('.bk-quiz__section-main').prev().removeClass('bk-quiz__section-title_error');
        }
    });

    $input.on('keyup', function () {
        if ($(this).val() !== '') {
            $(this).closest('.bk-input').removeClass('bk-input_error');
            $(this).closest('.bk-input').closest('.bk-quiz__section-main').prev().removeClass('bk-quiz__section-title_error');
        }
    });
});

$(function () {
    var MIN_LENGTH = 3;

    $('.header-search').each(function () {
        var $root = $(this);
        var $close = $root.find('.header-search__close');
        var $form = $root.find('.header-search__input form');
        var $input = $root.find('.header-search__input input');
        var $body = $root.find('.header-search__body');

        var request = void 0;
        var closeTimer = void 0;

        function open() {
            $root.addClass('header-search_show').reflow().addClass('header-search_opened');
            $input.focus();
            clearTimeout(closeTimer);
        }

        function close() {
            $root.removeClass('header-search_opened');
            clearTimeout(closeTimer);
            closeTimer = setTimeout(function () {
                $root.removeClass('header-search_show');
            }, 300);
        }

        function buildData(value) {
            return {
                ajax_call: 'y',
                INPUT_ID: 'search-input',
                q: value,
                l: MIN_LENGTH
            };
        }

        function load(value) {
            if (request) {
                request.abort();
            }

            request = $.ajax({
                url: '/ajax/search.php',
                type: 'post',
                dataType: 'html',
                data: buildData(value),
                success: function success(response) {
                    $body.html(response);
                },
                complete: function complete() {}
            });
        }

        $close.on('click', function (e) {
            e.preventDefault();
            close();
        });

        $('[data-js-header-search]').on('click', function (e) {
            e.preventDefault();
            open();
        });

        $input.on('keyup', function () {
            var value = $input.val().trim();

            if (value && value.length >= MIN_LENGTH) {
                load(value);
            } else {
                if (request) {
                    request.abort();
                }
                $body.empty();
            }
        });
    });
});

$(function () {

    $('.profile').each(function () {
        var $root = $(this);
        var $form = $root.find('form');

        $form.validate({
            highlight: function highlight(element) {
                $(element).addClass('error');
            },
            unhighlight: function unhighlight(element) {
                $(element).removeClass('error').addClass('success');
            },
            submitHandler: function submitHandler(form) {
                if ($form.valid()) {
                    $form.stop().animate({ opacity: .5 }, 600);
                    $form.find('button[type="submit"]').attr('disabled', 'disabled');
                    $form.find('.form__error').empty();
                    $.ajax({
                        type: 'post',
                        url: $form.attr('action'),
                        dataType: 'html',
                        data: $form.serialize(),
                        success: function success(response) {
                            var $response = $(response);
                            var $error = $response.find('.errortext');
                            var $success = $response.find('.notetext');
                            if ($error.length) {
                                $form.find('.form__error').html($error.html());
                            }
                            if ($success.length) {
                                $form.find('.form__success-inline').html($success.html());
                                $form.find('input[name="NEW_PASSWORD"], input[name="NEW_PASSWORD_CONFIRM"]').val('');
                            }
                        },
                        complete: function complete() {
                            $form.stop().animate({ opacity: 1 }, 300);
                            $form.find('button[type="submit"]').removeAttr('disabled');
                        }
                    });
                }
            },
            errorPlacement: function errorPlacement(error, element) {
                var $section = $(element).closest('.form__section');
                var $error = $section.find('.form__section-item_error');
                $error.append(error);
            }
        });

        $form.find(".password").rules("add", {
            messages: {
                regexp: BX.message('JS_PASS')
            }
        });
        $form.find(".confirm_password").rules("add", {
            equalTo: '.profile .password'
        });
    });
});

$(function () {

        $('[js-bk-rating]').each(function () {

                var $rating = $(this),
                    $input = $rating.find('input'),
                    $ratingBox = $('<div class="bk-rating__wrapper"></div>'),
                    defaultRating = $input.val(),
                    maxRating = $input.data('count');

                //add box
                $rating.prepend($ratingBox);

                //init
                var $wrapper = $rating.find('.bk-rating__wrapper');
                for (var i = 1; i <= maxRating; i++) {
                        var _$star = $('<div class="bk-rating__item"><div data-value="' + i + '" class="bk-rating__item-star bk-rating__item-star_not-active"></div></div>');
                        $wrapper.append(_$star);
                }

                //check active

                var $star = $wrapper.find('.bk-rating__item-star');

                $star.each(function () {
                        if ($(this).data('value') <= defaultRating) {
                                $(this).removeClass('bk-rating__item-star_not-active');
                                $(this).addClass('bk-rating__item-star_active');
                        }
                });

                //click star

                $star.on('click', function () {
                        var value = $(this).data('value');
                        $star.removeClass('bk-rating__item-star_active').addClass('bk-rating__item-star_not-active').each(function () {

                                if ($(this).data('value') <= value) {
                                        $(this).removeClass('bk-rating__item-star_not-active').addClass('bk-rating__item-star_active');
                                }
                        });

                        $input.val(value);
                });
        });
});

$(document).ready(function () {
    var obj = $('.question-bar');
    if (obj) {
        var windowHeight = window.innerHeight;
        $(window).scroll(function () {
            var scrollTop = $(window).scrollTop();
            if (scrollTop >= windowHeight) {
                obj.addClass('compact');
            }
        });
        $(document).on('click', '[data-js-activate-right-bar]', function (e) {
            e.preventDefault();
            var bar = $(this).closest('.question-bar').addClass('active');
            setTimeout(function () {
                bar.removeClass('active');
            }, 60000);
            $(window).scroll(function () {
                bar.removeClass('active');
            });
        });
    }
});

/**
 * Created by developer1 on 18.05.2017.
 */
(function () {
    $(document).ready(function () {
        var $object = $('[js-services-categories]');
        var $items = $object.find('[js-services-categories-item]').not('.active');
        var $close_btn = $object.find('[data-close]');
        var timeoutAId;
        var timeoutBId;
        $items.hover(function() {
                if (!timeoutAId) {
                    timeoutAId = window.setTimeout(function(section) {
                        timeoutAId = null;
                        section.addClass('opened');
                    }, 450, $(this));
                }
            },
            function () {
                if (timeoutAId) {
                    window.clearTimeout(timeoutAId);
                    timeoutAId = null;
                }
                else {
                    if (!timeoutBId) {
                        timeoutBId = window.setTimeout(function(section) {
                            timeoutBId = null;
                            section.removeClass('opened');
                        }, 450, $(this));
                    }
                }
            });
        $close_btn.on('click', function (e) {
            e.preventDefault();
            var $this = $(this);
            var $item = $this.closest('[js-services-categories-item]');
            $item.removeClass('opened').off('.open-description');
            setTimeout(function () {
                $item.on('mouseout.activate-close', function (e) {
                    e.stopPropagation();
                    if ($(e.target).is('a') || $(e.target).is('li')) {
                        $item.on('mouseover.open-description', function (e) {
                            e.stopPropagation();
                            $(this).addClass('opened');
                        });
                        $item.off('.activate-close');
                    }
                });
            }, 500);
        });
    });
})();


$(function () {
    var MIN_LENGTH = 3;

    $('.service-search').each(function () {
        var $root = $(this);
        var $close = $root.find('.service-search__close');
        var $form = $root.find('.service-search__input form');
        var $input = $root.find('.service-search__input input');
        var $body = $root.find('.service-search__body');

        var request = void 0;
        var closeTimer = void 0;

        function open() {
            $root.addClass('service-search_opened');
        }

        function close() {
            $root.removeClass('service-search_opened');
        }

        function check() {
            if ($body.html().trim()) {
                open();
            } else {
                close();
            }
        }

        function buildData(value) {
            return {
                ajax_call: 'y',
                INPUT_ID: 'search-input',
                q: value,
                l: MIN_LENGTH,
                serviceMode: 1
            };
        }

        function load(value) {
            if (request) {
                request.abort();
            }

            request = $.ajax({
                url: '/ajax/search.php',
                type: 'post',
                dataType: 'html',
                data: buildData(value),
                success: function success(response) {
                    $body.html(response);
                    check();
                },
                complete: function complete() {}
            });
        }

        $close.on('click', function (e) {
            e.preventDefault();
            close();
        });

        $input.on('keyup', function () {
            var value = $input.val().trim();

            if (value && value.length >= MIN_LENGTH) {
                load(value);
            } else {
                if (request) {
                    request.abort();
                }
                $body.empty();
                check();
            }
        });
    });
});

$(function () {

    var $servicesMenu = $('.js-services-menu'), // Это выпадающая часть меню Услуг
        $servicesElem = $('.js-menu-services'), // Это сама ссылка меню услуг
        $closeButton = $('.js-services-menu-close'),
        timeout,
        mouseOnMenu = false;

    $servicesElem.on('mouseenter', function () {
        // Навели на пункт меню
        clearTimeout(timeout);
        mouseOnMenu = false;
        timeout = setTimeout(function () {
            $servicesElem.addClass('menu__link_hover');
            $servicesMenu.addClass('services-menu_opened');
        }, 30);
    }).on('mouseleave', function (e) {
        if ($(e.toElement).hasClass('menu__link') && !$(e.toElement).hasClass("js-menu-services")) {
            // Закрываем быстро блок меню услуг
            // если было наведение на другой пункт меню,
            // но не родительский
            timeout = setTimeout(function () {
                if (!mouseOnMenu) {
                    $servicesElem.removeClass('menu__link_hover');
                    $servicesMenu.removeClass('services-menu_opened');
                }
            }, 30);
        } else {
            clearTimeout(timeout);
            timeout = setTimeout(function () {
                if (!mouseOnMenu) {
                    $servicesElem.removeClass('menu__link_hover');
                    $servicesMenu.removeClass('services-menu_opened');
                }
            }, 1000);
        }
    });

    $servicesMenu.on('mouseenter', function () {
        // Навели блок услуг
        clearTimeout(timeout);
        mouseOnMenu = true;
    }).on('mouseleave', function (e) {
        if ($(e.toElement).hasClass('menu__link') && !$(e.toElement).hasClass("js-menu-services")) {
            // Закрываем быстро блок меню услуг
            // если было наведение на другой пункт меню,
            // но не родительский
            timeout = setTimeout(function () {
                mouseOnMenu = false;
                $servicesElem.removeClass('menu__link_hover');
                $servicesMenu.removeClass('services-menu_opened');
            }, 30);
        } else {
            clearTimeout(timeout);
            timeout = setTimeout(function () {
                // Спрятали блок услуг
                mouseOnMenu = false;
                $servicesElem.removeClass('menu__link_hover');
                $servicesMenu.removeClass('services-menu_opened');
            }, 1000);
        }
    });

    $closeButton.on('click', function (e) {
        e.preventDefault();
        $servicesMenu.removeClass('services-menu_opened');
        $servicesElem.removeClass('menu__link_hover');
    });
});

(function ($) {
    $(document).ready(function () {
        var $slider = $("[data-owl-event-carousel]");
        $slider.owlCarousel({
            items: 3,
            loop: true,
            margin: 25,
            dots: false,
            nav: true,
            navText: [' ', ' '],
            onInitialized: function onInitialized(event) {
                var $items = $(event.target).find('.owl-item [data-owl-item]'),
                    $dots = $slider.find('.owl-dot');
                $items.each(function (i) {
                    var $this = $(this);
                    $dots.eq(i).css('background-image', 'url(' + $this.data('preview') + ')');
                });
            }
        });
    });
})(jQuery);

