	});

	/*bookmarks action*/
	$(document).on('click', '.bookmark_action', function () {
	    var _this = $(this);
		var serviceId = _this.data('id');
		var action = _this.data('action');
		var page = '/ajax/bookmark.php';
		var params = {
			'BOOKMARK_ACTION' : action,
			'ID' : serviceId
		};
        wait = BX.showWait();
        BX.ajax({
            url: page + '?' + $.param(params),
            method: 'GET',
            dataType: 'json',
            timeout: 30,
            async: true,
            processData: false,
            scriptsRunFirst: false,
            emulateOnload: false,
            start: true,
            cache: true,
            onsuccess: function(data){
                BX.closeWait(wait);
                var result = JSON.parse(data);

                if(action === 'PROCESS') {
					if(result['result']) {
						var l = $('.layer[data-id=bookmarks]');
						l.find('.bookmarks-wrapper').hide();
                        l.find('.bookmarks-result').show();
					}
				}
				updateBookmarks(result, params, _this);
            },
            onfailure: function(){
                BX.closeWait(wait);
                alert('Не удалось загрузить данные');
            }
        });
        return false;
    });

    function updateBookmarks(data, params, el){
        var textReplace = {
            ADD: '+ в мой список услуг',
            DELETE: 'Убрать из списка'
        };
        var elem = $('.i-link.bookmark_action'),
        	form = $('.bookmark_form');

        if(params['BOOKMARK_ACTION'] === 'PROCESS') {
            data.count = 0;
		}

        form.show();
        if(parseInt(data.count) === 0) {
            form.hide();
        }

        console.log(data.count);
        var bm_table_list = $('#bookmarks-table__list');
        if (data.count > 0) {
			$('.item-menu__bookmarks').text(data.count).closest('.question-bar__item_bookmarks').removeClass('question-bar__item_bookmarks_zero');
        } else {
        	$('.item-menu__bookmarks').text(data.count).closest('.question-bar__item_bookmarks').addClass('question-bar__item_bookmarks_zero');
        }

        bm_table_list.html('');


        if(elem.length > 0) {
            var rAction = (elem.data('action') === 'ADD') ? 'DELETE' : 'ADD';
            var appeal_btn = $('[data-group-button=send-order]');

            if (!!data.items && !!data.items[elem.data('id')]) {
                rAction = 'DELETE';
                elem.addClass('marked');
                appeal_btn.text(appeal_btn.data('marked_text')).attr('disabled', true).addClass('marked');
			} else {
                rAction = 'ADD';
                elem.removeClass('marked');
                appeal_btn.html(appeal_btn.data('default_text')).attr('disabled', false).removeClass('marked');
			}

            elem.text(textReplace[rAction]);
            if(rAction === 'ADD') {
            	elem.append('<span class="bm_additional">Вы можете создать свой список из нескольких услуг сразу и направить его нам на рассмотрение.</span>');
			}
            elem.data('action', rAction);
        }

        if(parseInt(data.count) !== 0) {
        	console.log({'data': data, 'params':params, 'el':el});
            var i = 0;

            for(var key in data.items){
                if(data.items.hasOwnProperty(key)){
                    var item = data.items[key];
                    var row = '<tr id="bookmarks_list_item_'+item['ID']+'">' +
                    '           <th>'+(++i)+'</th>' +
                    '           <th><a href="'+item['DETAIL_PAGE_URL']+'">'+item['NAME']+'</a></th>' +
                    '           <th><span class="red-close bookmark_action" data-id="'+item['ID']+'" data-action="DELETE"></span></th>' +
                    '          </tr>';

                    bm_table_list.append(row);
                }
            }
        }
    }

    $(document).on('click', '.question-bar__item_bookmarks', function () {
        var _this = $(this);
        var page = '/ajax/bookmark.php';
        var params = {
            'BOOKMARK_ACTION' : 'LIST'
        };
        wait = BX.showWait();
        BX.ajax({
            url: page + '?' + $.param(params),
            method: 'GET',
            dataType: 'json',
            timeout: 30,
            async: true,
            processData: false,
            scriptsRunFirst: false,
            emulateOnload: false,
            start: true,
            cache: true,
            onsuccess: function(data){
                BX.closeWait(wait);
                var result = JSON.parse(data);
                updateBookmarks(result, params, _this);
            },
            onfailure: function(){
                BX.closeWait(wait);
                alert('Не удалось загрузить данные');
            }
        });
    });
});

// Events

var timerScroll = false, ignoreScroll = [];
$(window).scroll(function(){
	if(!ignoreScroll.length){
		if(timerScroll){
			clearTimeout(timerScroll);
			timerScroll = false;
		}
		timerScroll = setTimeout(function(){
			BX.onCustomEvent('onWindowScroll', false);
		}, 100);
	}
});

var timerResize = false, ignoreResize = [];
$(window).resize(function(){
	if(!ignoreResize.length){
		if(timerResize){
			clearTimeout(timerResize);
			timerResize = false;
		}
		timerResize = setTimeout(function(){
			BX.onCustomEvent('onWindowResize', false);
		}, 100);
	}
});

BX.addCustomEvent('onWindowScroll', function(eventdata) {
	try{
		ignoreScroll.push(true);
		CheckPopupTop();
	}
	catch(e){}
	finally{
		ignoreScroll.pop();
	}
});

BX.addCustomEvent('onWindowResize', function(eventdata) {
	try{
		ignoreResize.push(true);
		CheckPopupTop();
		// CheckTopMenuDotted();
		CheckTopVisibleMenu();
		CheckFlexSlider();
		CheckMainBannerSliderVText($('.banners-big .flexslider'));
	}
	catch(e){}
	finally{
		ignoreResize.pop();
	}
});

BX.addCustomEvent('onSlide', function(eventdata) {
	try{
		ignoreResize.push(true);
		if(eventdata){
			var slider = eventdata.slider;
			if(slider){
				// add classes .curent & .shown to slide
				slider.find('.item').removeClass('current');
				var curSlide = slider.find('.item').eq(slider.currentSlide);
				curSlide.addClass('current');
				curSlide.addClass('shown');
				slider.resize();

				// set main banners text vertical center
				CheckMainBannerSliderVText(slider);
			}
		}
	}
	catch(e){}
	finally{
		ignoreResize.pop();
	}
});

