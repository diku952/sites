; /* Start:"a:4:{s:4:"full";s:55:"/local/templates/agima-main/js/plugins.js?1496263712784";s:6:"source";s:41:"/local/templates/agima-main/js/plugins.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
// Avoid `console` errors in browsers that lack a console.
(function() {
    var method;
    var noop = function () {};
    var methods = [
        'assert', 'clear', 'count', 'debug', 'dir', 'dirxml', 'error',
        'exception', 'group', 'groupCollapsed', 'groupEnd', 'info', 'log',
        'markTimeline', 'profile', 'profileEnd', 'table', 'time', 'timeEnd',
        'timeline', 'timelineEnd', 'timeStamp', 'trace', 'warn'
    ];
    var length = methods.length;
    var console = (window.console = window.console || {});

    while (length--) {
        method = methods[length];

        // Only stub undefined methods.
        if (!console[method]) {
            console[method] = noop;
        }
    }
}());

// Place any jQuery/helper plugins in here.

/* End */
;
; /* Start:"a:4:{s:4:"full";s:54:"/local/templates/agima-main/js/main.js?161884006841602";s:6:"source";s:38:"/local/templates/agima-main/js/main.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
$(function () {
    svg4everybody();

    if (window.location.hash) {
        var hash = window.location.hash.substring(1);
        if (hash === 'register') {
            setTimeout(function () {
                $('.body-header__office').trigger('click');
                setTimeout(function () {
                    $('[data-link=".auth-registration"]').trigger('click');
                }, 300);
            }, 200);
        }
    }

    $(document).on('click', '.top-section-indexes__close', function (e) {
        e.preventDefault();
        $(this).closest('.top-section').slideUp();
        document.cookie = 'ts-close=Y; path=/;';
        return false;
    });

    if ($('.fancybox').length || $('.fancybox-padding').length) {
        $('.fancybox').fancybox({
            padding: 0,
            margin: 50,
            scrolling: 'no',
            afterLoad: function (current, previous) {
                var callback = window[$(this.element).data('afterload')];
                if (typeof callback === 'function') {
                    callback(this, this.element);
                }
            },
        });

        $('.fancybox-padding').fancybox({
            padding: 30,
            afterLoad: function (current, previous) {
                var callback = window[$(this.element).data('afterload')];
                if (typeof callback === 'function') {
                    callback(this, this.element);
                }
            },
        });
    }

    if ($('.owl-carousel').length) {
        $('.owl-carousel').each(function () {
            if ($(this).hasClass('video-slider')) return;
            var data = {
                amount: $(this).data('slides-visible'),
                autoplay: $(this).data('autoplay'),
                nav: $(this).data('nav') ? $(this).data('nav') : false,
            };

            $(this).owlCarousel({
                items: data.amount ? data.amount : 1,
                autoplay: data.autoplay == false ? data.autoplay : true,
                autoplayTimeout: 10000,
                smartSpeed: 700,
                loop: true,
                nav: data.nav,
            });
        });
    }

    if (document.querySelector('.tooltip')) {
        $('.tooltip').tooltipster({
            functionInit: function (instance, helper) {
                var elSelector = instance._$origin.data('tooltip');
                var content = $('#' + elSelector);
                instance.content(content);
            },
            theme: 'tooltipster-light',
            IEmin: 9,
        });
    }

    ///////////////////////////////////////
    //        Н а в и г а ц и я          //
    ///////////////////////////////////////

    (function () {
        var links = $('.js-nav-nested-link');

        links.each(function (index, el) {
            var tm;
            var all = $('.js-nav-nested-list');
            var nested = $(this).siblings('.js-nav-nested-list');

            $(this).hover(
                function () {
                    clearTimeout(tm);
                    if (nested.is(':animated')) nested.stop().show();
                    all.not(nested).fadeOut(50);
                    nested.fadeIn(200);
                },
                function () {
                    tm = setTimeout(function () {
                        nested.fadeOut(200);
                    }, 450);
                }
            );

            nested.hover(
                function () {
                    clearTimeout(tm);
                },
                function () {
                    tm = setTimeout(function () {
                        nested.fadeOut(200);
                    }, 450);
                }
            );
        });
    })();

    /////////////////////////////////////////////////////////////
    //      Выравнивание колонок новостей в Пресс-центре       //
    /////////////////////////////////////////////////////////////

    (function () {
        var newsLists = $('.js-news-list');
        if (!newsLists.length) return;

        $(window).resize(throttle(onResize, 100));
        onResize();
        $(window).load(function () {
            onResize();
        });

        function onResize() {
            var things = [];
            $('.js-news-list-equalize').removeAttr('style');
            for (var i = 0; i < newsLists.length; i++) {
                things[i] = newsLists.eq(i).find('.js-news-list-equalize');
            }
            var l = things[0].length;
            var row;
            for (var j = 0; j < l; j++) {
                row = $('');
                for (var i = 0; i < newsLists.length; i++) {
                    row = row.add(things[i].eq(j));
                }
                equalizeHeight(row);
            }
        }

        function equalizeHeight(elems) {
            var maxH = 0;
            var h;
            for (var i = 0; i < elems.length; i++) {
                h = elems.eq(i).outerHeight();
                var z = elems.eq(i).find('.news-list_item-title').outerHeight();
                if (h > maxH) maxH = h;
            }
            elems.height(maxH);
        }
    })();

    ///////////////////////////////////
    //      A c c o r d i o n        //
    ///////////////////////////////////

    (function () {
        var panels = $('.js-panel');
        if (!panels.length) return;

        var panelToggle = panels.find('.js-panel-toggle');
        panelToggle.on('click', function () {
            $(this).parent('.js-panel').toggleClass('__active');
            $(this).siblings('.js-panel-body').slideToggle(200);
        });
    })();

    ////////////////////////////////////////////////
    //     Г р а ф и к   на   г л а в н о й       //
    ////////////////////////////////////////////////

    (function () {
        var graph = $('.js-export-graph');
        if (!graph.length) return;

        // Подсветка иконок и соотв. пункта
        var icons = graph.find('.main_export-icon');
        var tm;
        icons.on('mouseenter focusin', function () {
            var _this = $(this);
            tm = setTimeout(function () {
                _this.parents('.js-export-graph').addClass('hovered-icon');
                _this.parent().addClass('hovered');
                _this
                    .parent()
                    .prevAll('.main_export-item')
                    .addClass('next-hovered');
            }, 200);
        });
        icons.on('mouseleave focusout blur touchleave', function () {
            clearTimeout(tm);
            $(this).parents('.js-export-graph').removeClass('hovered-icon');
            $(this).parent().removeClass('hovered');
            $(this)
                .parent()
                .prevAll('.main_export-item')
                .removeClass('next-hovered');
        });

        // Подсветка текстовых областей
        $('.js-export-graph-group-text').each(function () {
            var descr = $(this).find('.js-export-graph-descr');
            var tm2, tm3;

            function showGroup(group) {
                if (group.hasClass('__1')) graph.addClass('hovered-1');
                else if (group.hasClass('__2')) graph.addClass('hovered-2');
                group.addClass('hovered');
                group.removeClass('hidden');
            }

            descr.on('mouseenter', function () {
                var group = $(this).parents('.js-export-graph-group-text');
                clearTimeout(tm3);
                if (!group.hasClass('hidden')) {
                    showGroup(group);
                } else {
                    tm2 = setTimeout(function () {
                        showGroup(group);
                    }, 400);
                }
            });
            descr.on('mouseleave', function (event) {
                clearTimeout(tm2);
                var group = $(this).parents('.js-export-graph-group-text');
                graph.removeClass('hovered-1 hovered-2');
                group.removeClass('hovered');
            });
        });

        // Раскрытие доп. пунктов списка
        var listLink = $('.js-export-work-toggle');
        var list = $('.js-export-work-list');
        listLink.on('click', function (event) {
            event.preventDefault();
            $(this).hide();
            list.slideDown(200);
        });
    })();

    ///////////////////////
    //      Select       //
    ///////////////////////
    /*var cs = $('.control_select');
    try {
        cs.select2({
            minimumResultsForSearch: Infinity
        });
    } catch (e) {
        console.error(e)
    }

    cs.on('select2:open', function (e) {
        var sdd = $('.select2-dropdown');
        sdd.hide();
        setTimeout(function () {
            sdd.slideDown('fast');
        }, 200);
    });*/

    //////////////////////////////
    //      Табы в формах       //
    //////////////////////////////
    (function () {
        $('.b-form_tabs.__radio input:checked')
            .parents('.b-form_tab ')
            .addClass('__checked');

        $('.b-form_tabs.__radio').on('change', 'input', function () {
            var parent = $(this).parents('.b-form_tab');
            parent.siblings().removeClass('__checked');
            parent.addClass('__checked');
        });
    })();

    // Форма загрузки файла в CRM
    $(document).on('reset', '.js-upload-form', function () {
        $(this).find('.upload-form_btn_label').text('Выберите файл...');
        $(this).find('input[type="submit"]').attr('disabled', 'true');
    });

    $(document).on('change', '.js-upload-form input[type="file"]', function () {
        var value = $(this).val() || '';
        var filedata = $(this).prop('files') || [];
        var form = $(this).parents('.js-upload-form');
        if (value.length > 0) {
            form.find('input[type="submit"]').removeAttr('disabled');
            var filesStr = '';
            for (var i = 0; i < filedata.length; i++) {
                filesStr += filedata[i].name + '<br>';
            }
            form.find('.upload-form_btn_label').html(filesStr);
        } else {
            form.find('input[type="submit"]').attr('disabled', 'true');
            form.find('.upload-form_btn_label').text('Выберите файл...');
        }
    });

    $('.js-upload-form').on('submit', function () {
        var currentform = $(this);
        var fileData =
            $(this).find('input[type="file"]').prop('files')[0] || '';
        var requestNum =
            $(this).find('input[name="requestNumber"]').val() || '';
        var appealId = $(this).find('input[name="appealId"]').val() || '';
        var fileId = $(this).find('input[name="fileId"]').val() || '';
        var method = $(this).attr('method') || '';
        var maxFileSize =
            parseInt($(this).find('input[name="maxFileSize"]').val()) || 0;
        var message = currentform.find('.message');
        var formData = new FormData();
        if (parseInt(fileData.size) > maxFileSize) {
            message.text(
                'Слишком большой файл, допустимый размер - ' +
                    maxFileSize / 1048576 +
                    ' mb'
            );
            currentform[0].reset();
            return false;
        }
        formData.append('file', fileData);
        formData.append('requestNum', requestNum);
        formData.append('appealId', appealId);
        formData.append('fileId', fileId);
        formData.append('method', method);
        ajaxFileSender(formData, currentform, message);
        return false;
    });

    $(document).on(
        'change',
        '.js-upload-form.with-request-form input[type="file"]',
        function () {
            var fileData = $(this).prop('files') || '';
            var needUploadVal = 'N';
            var form = $(this).parents('.js-upload-form');
            var maxFileSize =
                parseInt(form.find('input[name="maxFileSize"]').val()) || 0;
            var message = form.find('.message');
            message.text('');

            for (var i = 0; i < fileData.length; i++) {
                if (parseInt(fileData[i].size) > maxFileSize) {
                    message.text(
                        'Слишком большой файл, допустимый размер - ' +
                            maxFileSize / 1048576 +
                            ' mb'
                    );
                    $(this).val('');
                    form.find('.upload-form_btn_label').html('Выберите файл');
                }
            }
            if (fileData.length > 0) {
                needUploadVal = 'Y';
                window.arFiles = fileData;
            }
            form.find('input[name="needToUploadFiles"]').val(needUploadVal);
        }
    );

    // END Форма загрузки файла в CRM
});

// End   Of   R E A D Y

function fileUploaderHandler(arFiles, requestNum, appealId, callback) {
    var uploaderBox = $('.js-fileuploader_box');
    var promises = [];
    $.each(arFiles, function (index, value) {
        var message = $('<p></p>');
        uploaderBox.append(message);
        var formData = new FormData();
        formData.append('requestNum', requestNum);
        formData.append('file', value);
        formData.append('method', 'post');
        formData.append('appealId', appealId);
        var promise = new Promise(function (resolve, reject) {
            ajaxFileSender(
                formData,
                uploaderBox,
                message,
                value,
                resolve,
                reject,
                true
            );
        });
        promises.push(promise);
    });
    Promise.all(promises).then(function () {
        callback();
    });
}

function ajaxFileSender(
    formData,
    currentform,
    message,
    file,
    resolve,
    reject,
    inRequestForm
) {
    var inRequestForm = inRequestForm || false;
    $.ajax({
        url: '/ajax/fileUploadController.php',
        dataType: 'text',
        cache: false,
        contentType: false,
        processData: false,
        data: formData,
        type: 'post',
        beforeSend: function () {
            if (inRequestForm) {
                message.text('Отправка файла ' + file.name + '.....');
            }
        },
        success: function (response) {
            var responseData = JSON.parse(response);
            if (responseData.hasOwnProperty('error')) {
                message.text(responseData.error);
            } else {
                if (
                    responseData.hasOwnProperty('fileId') &&
                    responseData.hasOwnProperty('result')
                ) {
                    if (inRequestForm) {
                        message.text(responseData.result);
                        resolve();
                    } else {
                        currentform[0].reset();
                        message.text(responseData.result);
                        currentform
                            .find('input[name="fileId"]')
                            .val(responseData.fileId);
                        currentform.attr('method', 'put');
                        currentform
                            .find('input[type="submit"]')
                            .attr('disabled', 'true');
                    }
                }
            }
        },
    }).fail(function () {
        reject('Fail file upload');
    });
}

///////////////////////
//    IE 9 FIXES     //
///////////////////////
if ($('html').hasClass('bx-ie9')) {
    $(function () {
        $('input, textarea').placeholder();
    });
}

// Helpers

function isScrolledIntoView(el) {
    var rect = el.getBoundingClientRect();
    var elemTop = rect.top;
    var elemBottom = rect.bottom;
    var isVisible = elemTop >= 0 && elemBottom <= window.innerHeight;
    // Partially visible elements return true:
    //isVisible = elemTop < window.innerHeight && elemBottom >= 0;
    return isVisible;
}

function throttle(callback, limit) {
    var wait = false;
    return function () {
        if (!wait) {
            callback.call();
            wait = true;
            setTimeout(function () {
                wait = false;
            }, limit);
        }
    };
}

$.fn.dadataSuggestions = function () {
    return this.each(function (i, item) {
        var $root = $(item);
        var $fio = $root.find('[js-dadata-fio], input[name="FIO"]');
        var $surname = $root.find('[js-dadata-surname]');
        var $name = $root.find('[js-dadata-name]');
        var $patronymic = $root.find('[js-dadata-patronymic]');
        var $party = $root.find(
            '[js-dadata-party], input[name="COMPANY_NAME"]'
        );
        var $okved = $root.find('.okved');
        var $tnved = $root.find('.tnved');
        var $ogrn = $root.find(
            '[js-dadata-ogrn], input[name="OGRN_INN"], input[name="OGRN"]'
        );
        var $address = $root.find('[js-dadata-address]');
        var $email = $root.find('[js-dadata-email]');

        var gender = 'UNKNOWN';

        function init($element, type, parts) {
            $element.suggestions({
                token: '4215da902c9838352063c6b9daaca039928c14aa',
                type: type,
                triggerSelectOnSpace: false,
                hint: '',
                noCache: true,
                params: {
                    parts: parts || [],
                },
                onSearchStart: function (params) {
                    switch (type) {
                        case 'NAME':
                            params.gender = gender;
                            break;
                        case 'PARTY':
                            break;
                    }
                },
                onSelect: function (suggestion) {
                    switch (type) {
                        case 'NAME':
                            gender = suggestion.data.gender;
                            break;
                        case 'PARTY':
                            if ($ogrn.length && suggestion.data.ogrn) {
                                $ogrn.val(suggestion.data.ogrn);
                            }
                            break;
                    }
                },
            });
        }

        if ($fio.length) {
            init($fio, 'NAME');
        }
        if ($okved.length > 0) {
            $okved.each(function (index, el) {
                var input = $('[name="OKVED_fake"]');
                var name = 'OKVED[]';
                var loader = $(el).find('.loader');
                var result = $(el).find('.field_result');
                result.find('.res_el').each(function (index, el) {
                    deleteResultElement($(el).find('button.delete'));
                    changeTotalPercentTnved($(el));
                });
                input.autocomplete({
                    serviceUrl: 'https://dadata.ru/api/v2/suggest/okved2',
                    //serviceUrl: $template_path + "/autocomplete.okved.hl.php",
                    params: {
                        token: '268235c5c930c9255863911700b9bb5c400d1a71',
                        type: 'okved',
                        sessid: BX.bitrix_sessid(),
                    },
                    onSearchStart: function (params) {},
                    /*onSuggestionsFetch: function(suggestions){
                    $.ajax({
                      async: false,
                      url: "/local/components/agima/form.designer/templates/.default/autocomplete.tnved.hl.php",
                      data: {
                        type: "tnved",
                        sessid: BX.bitrix_sessid(),
                        query: this.currentValue
                      },
                      success: function(data){
                        if(!data.error){
                          data = data.suggestions;
                          for(var i in data){
                            suggestions.push({
                              value: data[i].description,
                              data: {
                                kod: data[i].data,
                                idx: data[i].value,
                                name: data[i].description
                              }
                            });
                          }
                        }
                      }
                    });
                    return suggestions;
                  },*/
                    onSearchComplete: function (query, suggestions) {
                        if (
                            suggestions.length === 1 &&
                            suggestions[0].value === ''
                        ) {
                            $(input).autocomplete('hide');
                        }
                    },
                    onSelect: function (suggestion) {
                        $tnved.find('.res_el .delete').trigger('click');
                        $tnved.closest('[data-sid="TNVED"]').hide();
                        if (result.find('.res_el').length < 5) {
                            if (
                                result.find('#res_el_' + suggestion.data.kod)
                                    .length > 0
                            ) {
                                input.data('value', input.val());
                                input.val('');
                                return;
                            }
                            if (suggestion.value === '') {
                                return;
                            }
                            var suggestionData = {
                                value: suggestion.data.idx,
                                description: suggestion.value,
                            };
                            var res_el =
                                "<div class='res_el' id='res_el_" +
                                suggestion.data.kod +
                                "'>" +
                                '<p><b>' +
                                suggestion.data.idx +
                                '</b> - ' +
                                '<span>' +
                                suggestion.value +
                                '</span></p>' +
                                "<input type='hidden' name='" +
                                name +
                                "' value='" +
                                suggestionData.value +
                                ' ' +
                                suggestionData.description +
                                "'>" +
                                "<button type='button' class='delete' data-id='res_el_" +
                                suggestion.data.kod +
                                "'>Удалить</button>" +
                                '</div>';
                            res_el = result.append(res_el);
                            var btn = res_el.find('button.delete');
                            $(btn).on('click', function () {
                                if (result.find('.res_el').length == 1) {
                                    $tnved.closest('[data-sid="TNVED"]').show();
                                }
                                var button = event.target;
                                $(button).closest('.res_el').remove();
                            });
                            input.data('value', input.val());
                            input.val('');
                            input.trigger('change');
                            input.trigger('onSelect');
                        }
                    },
                    formatResult: function (suggestion, currentValue) {
                        if (!currentValue) {
                            return suggestion.value;
                        }

                        var pattern =
                            '(' +
                            currentValue.replace(
                                /[|\\{}()[\]^$+*?.]/g,
                                '\\$&'
                            ) +
                            ')';
                        return (suggestion.data.idx + '<br>' + suggestion.value)
                            .replace(
                                new RegExp(pattern, 'gi'),
                                '<strong>$1</strong>'
                            )
                            .replace(/&lt;(\/?strong)&gt;/g, '<$1>');
                    },
                    transformResult: function (response, originalQuery) {
                        var result = {};
                        if (typeof response === 'string') {
                            result = $.parseJSON(response);
                            if (result.error) {
                                input.after(
                                    "<div class='field_error'>Внутреняя ошибка</div>"
                                );
                                return result;
                            }
                            if (
                                result.suggestions.length === 1 &&
                                result.suggestions[0].value === ''
                            ) {
                                input.after(
                                    "<div class='field_error'>Для продукции с кодом " +
                                        originalQuery +
                                        ' данная услуга недоступна.</div>'
                                );
                                return result;
                            }
                        } else {
                            result = response;
                        }
                        return result;
                    },
                });
            });
        }
        if ($tnved.length > 0) {
            var special_tnved = $('.tnved');

            var input = $('[name="TNVED_fake"]');
            var name = 'TNVED[]';
            var loader = $(special_tnved).find('.loader');
            var result = $(special_tnved).find('.field_result');
            var limitation = special_tnved.attr('data-limitation');
            result.find('.res_el').each(function (index, el) {
                deleteResultElement($(el).find('button.delete'));
                changeTotalPercentTnved($(el));
            });
            input.autocomplete({
                serviceUrl:
                    '/local/components/agima/form.designer/templates/.default/autocomplete.tnved.hl.php',
                params: {
                    type: 'tnved',
                    sessid: BX.bitrix_sessid(),
                },
                onSearchStart: function (params) {},
                onSuggestionsFetch: function (suggestions) {
                    if (limitation > 0) {
                        var newSuggestions = [];
                        for (var i = 0; i < suggestions.length; i++) {
                            if (suggestions[i].value.length == limitation) {
                                newSuggestions.push(suggestions[i]);
                            }
                        }
                        return newSuggestions;
                    } else {
                        return suggestions;
                    }
                },
                onSearchComplete: function (query, suggestions) {
                    if (
                        suggestions.length === 1 &&
                        suggestions[0].value === ''
                    ) {
                        $(input).autocomplete('hide');
                    }
                },
                onSelect: function (suggestion) {
                    $okved.find('.res_el .delete').trigger('click');
                    $okved.closest('[data-sid="OKVED"]').hide();
                    if (result.find('#res_el_' + suggestion.data).length > 0) {
                        input.data('value', input.val());
                        input.val('');
                        return;
                    }
                    if (result.find('.res_el').length >= this.dataset.limit) {
                        input.data('value', input.val());
                        input.val('');
                        return;
                    }
                    if ($('#res_el_' + suggestion.data).length > 0) {
                        input.val('');
                        return;
                    }
                    if (suggestion.value === '') {
                        return;
                    }
                    var res_el =
                        "<div class='res_el' id='res_el_" +
                        suggestion.data +
                        "'>" +
                        '<p><b>' +
                        suggestion.value +
                        '</b> - ' +
                        '<span>' +
                        suggestion.description +
                        '</span></p>' +
                        "<input type='hidden' name='" +
                        name +
                        "' value='" +
                        suggestion.value +
                        ' ' +
                        suggestion.description +
                        "'>" +
                        //"<input type='text' class='i-input__input percent' placeholder='Доля от общей стоимости, %'>" +
                        "<button type='button' class='delete' data-id='res_el_" +
                        suggestion.data +
                        "'>Удалить</button>" +
                        '</div>';
                    res_el = result.append(res_el);
                    var btn = res_el.find('button.delete');
                    $(btn).on('click', function (event) {
                        if (result.find('.res_el').length == 1) {
                            $okved.closest('[data-sid="OKVED"]').show();
                        }
                        var button = event.target;
                        $(button).closest('.res_el').remove();
                    });
                    input.data('value', input.val());
                    input.val('');
                    input.trigger('change');
                    input.trigger('onSelect');
                },
                formatResult: function (suggestion, currentValue) {
                    if (!currentValue) {
                        return suggestion.value;
                    }

                    var pattern =
                        '(' +
                        currentValue.replace(/[|\\{}()[\]^$+*?.]/g, '\\$&') +
                        ')';
                    return (suggestion.value + '<br>' + suggestion.description)
                        .replace(
                            new RegExp(pattern, 'gi'),
                            '<strong>$1</strong>'
                        )
                        .replace(/&lt;(\/?strong)&gt;/g, '<$1>');
                },
                transformResult: function (response, originalQuery) {
                    var result = {};
                    if (typeof response === 'string') {
                        result = $.parseJSON(response);
                        if (result.error) {
                            input.after(
                                "<div class='field_error'>Внутреняя ошибка</div>"
                            );
                            return result;
                        }
                        if (
                            result.suggestions.length === 1 &&
                            result.suggestions[0].value === ''
                        ) {
                            input.after(
                                "<div class='field_error'>Для продукции с кодом " +
                                    originalQuery +
                                    ' данная услуга недоступна.</div>'
                            );
                            return result;
                        }
                    } else {
                        result = response;
                    }
                    return result;
                },
            });
        }
        if ($surname.length) {
            init($surname, 'NAME', ['SURNAME']);
        }
        if ($name.length) {
            init($name, 'NAME', ['NAME']);
        }
        if ($patronymic.length) {
            init($patronymic, 'NAME', ['PATRONYMIC']);
        }
        if ($party.length) {
            init($party, 'PARTY');
        }
        if ($address.length) {
            init($address, 'ADDRESS');
        }
        if ($email.length) {
            init($email, 'EMAIL');
        }
    });
};

$(function () {
    if ($('[js-dadata]').lenght > 0) {
        $('[js-dadata]').dadataSuggestions();
    }
});

function clearInputFile(f) {
    if (f.value) {
        try {
            f.value = ''; //for IE11, latest Chrome/Firefox/Opera...
        } catch (err) {}
        if (f.value) {
            //for IE5 ~ IE10
            var form = document.createElement('form'),
                parentNode = f.parentNode,
                ref = f.nextSibling;
            form.appendChild(f);
            form.reset();
            parentNode.insertBefore(f, ref);
        }
    }
}

function upload_file_ajax(formData, action, result, callback) {
    var xhr = new XMLHttpRequest();

    xhr.upload.onprogress = function (event) {
        console.log(event.loaded + ' / ' + event.total);
    };

    xhr.onload = xhr.onerror = function () {
        if (this.status == 200) {
            if (this.response.indexOf('errortext') + 1) {
                $(result[0]).html(this.response);
            } else {
                $(result[1]).html(this.response);
            }
        } else {
            console.log('error ' + this.status);
        }

        if (callback && typeof callback === 'function') {
            callback();
        }
    };

    xhr.open('POST', action, true);
    xhr.send(formData);
}

/*loader*/

var lastWait = [];
/* non-xhr loadings */
BX.showWait = function (node, msg) {
    node = BX(node) || document.body || document.documentElement;
    msg = msg || BX.message('JS_CORE_LOADING');

    var container_id = node.id || Math.random();

    var obMsg = (node.bxmsg = document.body.appendChild(
        BX.create('DIV', {
            props: {
                id: 'wait_' + container_id,
                className: 'bx-core-waitwindow',
            },
            text: msg,
        })
    ));

    setTimeout(BX.delegate(_adjustWait, node), 10);

    $('#preloader_wrapper').show();
    $('.rec-preloader').fadeIn(200);
    lastWait[lastWait.length] = obMsg;
    return obMsg;
};

BX.closeWait = function (node, obMsg) {
    $('#preloader_wrapper').hide();
    $('.rec-preloader').fadeOut(200);
    if (node && !obMsg) obMsg = node.bxmsg;
    if (node && !obMsg && BX.hasClass(node, 'bx-core-waitwindow')) obMsg = node;
    if (node && !obMsg) obMsg = BX('wait_' + node.id);
    if (!obMsg) obMsg = lastWait.pop();

    if (obMsg && obMsg.parentNode) {
        for (var i = 0, len = lastWait.length; i < len; i++) {
            if (obMsg == lastWait[i]) {
                lastWait = BX.util.deleteFromArray(lastWait, i);
                break;
            }
        }

        obMsg.parentNode.removeChild(obMsg);
        if (node) node.bxmsg = null;
        BX.cleanNode(obMsg, true);
    }
};

function _adjustWait() {
    if (!this.bxmsg) return;

    var arContainerPos = BX.pos(this),
        div_top = arContainerPos.top;

    if (div_top < BX.GetDocElement().scrollTop)
        div_top = BX.GetDocElement().scrollTop + 5;

    this.bxmsg.style.top = div_top + 5 + 'px';

    if (this == BX.GetDocElement()) {
        this.bxmsg.style.right = '5px';
    } else {
        this.bxmsg.style.left =
            arContainerPos.right - this.bxmsg.offsetWidth - 5 + 'px';
    }
}

function Tabs(opts) {
    var self = this;

    self.opts = opts;

    self.init = function () {
        self.opts.$links.on('click', function (e) {
            e.preventDefault();
            var group = $(this).data('group');
            self.switchTab(group);
        });
    };

    self.switchTab = function (group) {
        self.opts.$links
            .removeClass('active')
            .filter('[data-group="' + group + '"]')
            .addClass('active');
        self.opts.$blocks
            .removeClass('active')
            .filter('[data-group="' + group + '"]')
            .addClass('active');
    };
}

$(document).ready(function () {
    var $tabs = $('.js-tabs');

    if ($tabs.length) {
        $tabs.each(function () {
            var $this = $(this);

            $this.data(
                'tabs',
                new Tabs({
                    $root: $this,
                    $links: $this.find('.js-tabs__link'),
                    $blocks: $this.find('.js-tabs__block'),
                })
            );

            $this.data('tabs').init();
        });
        setTimeout(function () {
            $('.js-tabs__link.active').click();
        }, 300);
    }

    var js_open_after = $('[data-open-after]');
    if (js_open_after.length && js_open_after.attr('data-open-after')) {
        setTimeout(function () {
            js_open_after.click();
        }, js_open_after.attr('data-open-after'));
    }
    //обёртывание всех таблиц в контенте новостей скролом
    $('.detail.news table').each(function () {
        $(this).wrap('<div class="scroll"></div>');
        $('.scroll').mCustomScrollbar({
            mouseWheel: true,
            axis: 'x',
        });
    });
    //Показываем тултипы таблицы по клику
    if ($('.int-markets-container').length) {
        $('.structure_block-name').on('click', function () {
            $(this).next('.structure_consist').show();
        });
        $(':not(.structure_block-name)').on('click', function () {
            $(this).next('.structure_consist').hide();
        });
    }

    //МОДАЛКА С ОПРОСОМ
