; /* Start:"a:4:{s:4:"full";s:98:"/local/templates/agima-main/components/qsoft/form.callback.phone/.default/script.js?15277874983527";s:6:"source";s:83:"/local/templates/agima-main/components/qsoft/form.callback.phone/.default/script.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
$(document).ready(function() {
	$.validator.addMethod(
		'regexp',
	    function(value, element, regexp) {
	        var re = new RegExp(regexp);
	        return this.optional(element) || re.test(value);
	    },
	    ""
	);
	$('form.callback_block_form').validate({
	  	rules: {
	    	E_MAIL: {
	    		email: true,
	    	},
	    	PHONE: {
	    		regexp:/^[\+\d\-\(\)]{4,15}$/
	    	},
	    	FIO: {
	    		regexp:/^[\w\sА-Яа-яёЁ]+$/
	    	},
	    	OGRN_INN: {
	    		regexp:/^([\d]{1,}[\s-]{0,1}){1,}$/
	    	}
	  	},
	  	messages: {
	    	E_MAIL: {
	    		required: '',
	    		email:'',
	    	},
	    	PHONE: {
	    		required: '',
	    		regexp:''
	    	},
	    	FIO: {
	    		required: '',
	    	},
		    captcha_word:{
			    required: ''
		    },
		    OGRN_INN: {
			    required: ''
		    }
	  	},
	  	submitHandler: function(form) {
	  		console.log('old handler');
			$.ajax({
				type: $('.callback_block_form').attr('method'),
				url: $('[name=\'ajaxurl\']').val(),
				data: getData('.callback_block'),
				success:function(data){
					success = false;
					data = JSON.parse(data);
					var html = '<span class="close_black"></span>'
					+'<div class="answer">';
					if(typeof data == 'object' && data !==null){
						for(var $key in data['errors']){
							$('[name="'+$key+'"]').addClass('error');
                            $('[name="'+$key+'"]').prev().text(data.errors[$key]).css({'color':'red', 'font-size': '10px'});
							$('[name="'+$key+'"]').one('click',function(){
								$('[name="'+$key+'"]').removeClass('error');
							});
						}
						$('#captcha_code').val(data['new_code']);
						$('#captcha_word').val('');
						$('#captcha_img').attr("src","/bitrix/tools/captcha.php?captcha_code="+data['new_code']);
					} else if (typeof data === 'string') {
						success = true;
						html += data;
					} else {
						$('.callback_block_form').append('<label class="error general_error">Извините, форма не отправлена, или ответ от сервиса не получен, попробуйте позже</label>');
						$('.callback_block').on('click',function(){
							$('.general_error').remove();
						});
					}
					html +='</div>';
					if(success){
                        $('.callback_block_form').remove();
                        $('.callback_block_popup').append(html);
                        $('.close_black').on('click',function(){
				            $('.callback_block_popup').css({'visibility': 'hidden'});
				            $('.callback_block_button').removeClass('active');
				        });
				    }
				},
			});
		}
	});

	var maskList = $.masksSort($.masksLoad("/local/templates/aspro-scorp_new/js/input_mask/phone-codes.json"), ['#'], /[0-9]|#/, "mask");
    var maskOpts = {
        inputmask: {
            definitions: {
                '#': {
                    validator: "[0-9]",
                    cardinality: 1
                }
            },
            clearIncomplete: false,
            showMaskOnHover: false,
            autoUnmask: true
        },
        match: /[0-9]/,
        replace: '#',
        list: maskList,
        listKey: "mask",
    };
    $('input[name="PHONE"]').inputmasks(maskOpts);
});

function getData(selector){
	var data = {};
	inputs = $(selector).find('input');
	inputs.each(function(i,elem){
		jqElem = $(elem);
		data[jqElem.attr('name')] = jqElem.val();
	});
	return data;
}

