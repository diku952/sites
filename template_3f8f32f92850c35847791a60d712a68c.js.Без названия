// Text scroll block
function scrollBlock(opts) {
    var self = this;

    self.opts = opts;

    self.init = function () {

        self.opts.$input.on('change', function () {
            self.opts.$preview.toggleClass('_hidden');
            self.opts.$text.toggleClass('_hidden');
            var $open = self.opts.$input.prop('checked');
            $open ? self.opts.$buttonText.text('Свернуть') : self.opts.$buttonText.text('Подробнее');
        });
    };
}
function scrollBlockIn() {
    var $block = $('.js-scroll-block');

    if ($block.length) {
        $block.each(function () {
            var $this = $(this);

            $this.data('scrollBlock', new scrollBlock({
                $root: $this,
                $preview: $this.find('.js-scroll-preview'),
                $text: $this.find('.js-scroll-text'),
                $button: $this.find('.js-scroll-button'),
                $input: $this.find('.js-scroll-button-input'),
                $buttonText: $this.find('.js-scroll-button-text')
            }));

            $this.data('scrollBlock').init();
        });
    }
}

// CustomScroll init
function scrollIn() {

    var $scroll = $('.js-scroll');
    $scroll.mCustomScrollbar({
        theme: 'rec'
    });
}

// ScrollTo.js init
function scrollToIn() {
    var $scrollTo = $('.js-scroll-to');

    if ($scrollTo.length) {
        $scrollTo.scrollTo();
    }
}

// Scroll to
function scrollNavIn() {

    var $navBlock = $('.js-nav-scroll');

    if ($navBlock.length) {
        var $link = $navBlock.find('span[data-scroll-item]'),
            $block = $('[data-scroll-block]');

        $link.each(function () {
            var $menuItem = $(this);

            $menuItem.on('click', function () {
                // add class to menu item
                $('span[data-scroll-item]').removeClass('_active');
                $(this).addClass('_active');

                // open hided text and change toggler text
                var openText = $('.js-toggle-link').attr('data-open-text');
                $('.js-toggle-block').removeClass('_hidden');
                $('.js-toggle-link').addClass('is-open').text(openText);

                // scroll to block
                var blockName = $(this).data('scroll-item');

                var $b = $block.filter('[data-scroll-block="' + blockName + '"]'),
                    $pos = $b.offset().top - 20;

                $('html, body').animate({ scrollTop: $pos }, 800);
            });
        });
    }
}

$(document).ready(function () {
    scrollIn();
    scrollBlockIn();
    //scrollToIn();
    scrollNavIn();
});
