        });
    });
})(jQuery);

(function ($) {
    $(document).ready(function () {
        var $slider = $(".site-slider-full__container"),
            // $slideContent = $(".site-slider-full__item__container__wrapper"),
            $dots,
            parent = $slider.parent(),
            offset;

        if (parent.length) {
            offset = parent.offset().left;
        }

        parent.css('left', -offset);

        $slider.owlCarousel({
            items: 1,
            loop: true,
            margin: 0,
            autoplay: true,
            autoplayTimeout: 10000,
            smartSpeed: 1000
        });

        setTimeout(function () {
            var padding = parseInt($('.site-slider-full__item__container').first().css('margin-left'));
            $(".site-slider-full__item__container__wrapper").css('margin-left', offset - padding);
        }, 100);

        $dots = $(".site-slider-full__container .owl-dots");

        $dots.css('left', offset);
    });
})(jQuery);

(function ($) {
    $(document).ready(function () {
        var $slider = $(".news-image-slider");
        $slider.owlCarousel({
            items: 1,
            loop: true,
            margin: 0,
            autoplay: true,
            autoplayTimeout: 6000,
            smartSpeed: 1000,
            dots: false,
            nav: false,
            URLhashListener: true,
            callbacks: true
        });

        $slider.on('change.owl.carousel', function(event) {
            setTimeout(function(){
                var hashIndex = $('.news-image-slider').find('.owl-item.active a').data('hash');
                $('.news-image-slider-dots [href="#'+hashIndex+'"]').addClass('active').siblings().removeClass('active');
            }, 400)
        });
    });
})(jQuery);

$(function () {
    $('[js-tab-constructor]').each(function (i) {
        var $this = $(this),
            $tabs_blocks = $('[data-tab-group=' + $this.attr('data-group') + ']');
        $tabs_blocks.each(function (i) {
            var $tab_block = $(this);
            $this.append('<a js-tab-activator="' + $tab_block.data('tab') + '" data-tab-group="' + $tab_block.data('tab-group') + '" href="#" class="site-news-tabs__tabs__item ' + ($tab_block.hasClass('active') ? 'active' : '') + '">' + $tab_block.data('title') + '</a>');
        });
    });
    $(document).on('click.tabs', '[js-tab-activator]', function (e) {
        e.preventDefault();
        var $this = $(this),
            group = $this.data('tab-group');
        $('[data-tab-group=' + group + ']').removeClass('active');
        $('[data-tab-group=' + group + '][data-tab=' + $this.attr('js-tab-activator') + ']').addClass('active');
        $this.addClass('active');
    });
});

$(document).ready(function () {
    var obj = $('.site-service-detail__share');
    if (obj.hasClass('site-service-detail__share')) {
        var offset = obj.offset();
        var topOffset = offset.top;
        var footerOffset = obj.parent().offset().top;
        var marginTop = obj.css("marginTop");

        $(window).scroll(function () {
            var scrollTop = $(window).scrollTop();

            if (scrollTop >= topOffset) {
                obj.addClass('sticky');
            }

            if (scrollTop < topOffset) {
                obj.removeClass('sticky');
            }
            if (scrollTop >= footerOffset + obj.parent().height() - obj.height()) {
                obj.addClass('sticky-bottom');
            }

            if (scrollTop < footerOffset + obj.parent().height() - obj.height()) {
                obj.removeClass('sticky-bottom');
            }
        });
    }
});

function TableTab(object) {
    // hide all tables
    var input = $(object).find('input[type="radio"]:first-child'),
        groupName = input.attr('name'),
        firstTab = input.attr('value');
    $('[data-tab-name="'+groupName+'"]').hide();
    // show first table
    $('[data-tab-name="'+groupName+'"][data-tab="'+firstTab+'"]').show();
    // Add event
    TableTab.prototype.addEvent(object);
}
TableTab.prototype.addEvent = function (object) {
    // allow click or change
    $(object).find('input[type="radio"]').on('change', function(e) {
        var tabGroup = e.currentTarget.name,
            tabName = e.currentTarget.value;
        $('[data-tab-name="'+tabGroup+'"]').hide();
        $('[data-tab-name="'+tabGroup+'"][data-tab="'+tabName+'"]').show();
    });
};
//# sourceMappingURL=common.js.map
$(document).ready(function () {
	$(document).on('click', '.layer__close, .layer__back, [js-close-layer], .jqmClose', function (e) {
		var authWindow = $('[data-link=".auth-auth"]');
		var regWindow = $('.js-layer-register');
        if ( authWindow.length > 0 && regWindow.hasClass('is-active') ) {
			setTimeout(function(){regWindow.removeClass('is-active');authWindow.addClass('is-active');}, 500)
        }
	});

	//faq section
    var faq_section = $('.faq-section');
    if (faq_section.length) {
        svg4everybody();
        faq_section.find('.faq-back-link').on('click', function (e) {
            e.preventDefault();
            window.history.go(-1);
            return false;
        });

        faq_section.find(".js-select").select2({
            theme: 'exportcenter'
        }).on('select2:open', function () {
            $('.select2-container--exportcenter .select2-dropdown--below').mCustomScrollbar({
                theme: 'rec',
                scrollInertia: 250,
                autoHideScrollbar: false,
                mouseWheel: true,
            });
            $('.select2-container--exportcenter').addClass('select-faq-section');
            $(".select2-container--exportcenter .select2-results ul.select2-results__options").unbind("mousewheel")
        });
        faq_section.find('.main-slider__list--situation').slick({
            infinite: true,
            slidesToScroll: 1,
            autoplay: true,
            autoplaySpeed: 4000,
            slidesToShow: 1,
            dots: false,
            arrows: false
        });

        faq_section.find('.main-slider__list--service').slick({
            infinite: true,
            slidesToScroll: 1,
            slidesToShow: 3,
            dots: false,
            arrows: true,
            prevArrow: $('.js-slider-service-prev'),
            nextArrow: $('.js-slider-service-next')
        });
        faq_section.find('#accordion [data-accordion]').accordion({
            singleOpen: false
        });
    }

    $('.faq-back-link').on('click', function (e) {
        e.preventDefault();
        window.history.go(-1);
        return false;
    });
});
/*
 метод для генерации переноса строк
 */
$.fn.hyphenate = function() {
    var all = "[абвгдеёжзийклмнопрстуфхцчшщъыьэюя]",
        glas = "[аеёиоуыэю\я]",
        sogl = "[бвгджзклмнпрстфхцчшщ]",
        zn = "[йъь]",
        shy = "\xAD",
        re = [];

    re[1] = new RegExp("("+zn+")("+all+all+")","ig");
    re[2] = new RegExp("("+glas+")("+glas+all+")","ig");
    re[3] = new RegExp("("+glas+sogl+")("+sogl+glas+")","ig");
    re[4] = new RegExp("("+sogl+glas+")("+sogl+glas+")","ig");
    re[5] = new RegExp("("+glas+sogl+")("+sogl+sogl+glas+")","ig");
    re[6] = new RegExp("("+glas+sogl+sogl+")("+sogl+sogl+glas+")","ig");
    console.log(this);
    return this.each(function() {
        var text = $(this).html();
        if (typeof text !== 'undefined') {
            for (var i = 1; i < 7; ++i) {
                text = text.replace(re[i], "$1"+shy+"$2");
            }
            $(this).html(text);
        }
    });
};
